<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.agency.Curriculum" table="CURRICULUM" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="relatedid" type="long" column="RELATEDID"/>
		<property name="fromdate" type="date" column="FROMDATE"/>
		<property name="todate" type="date" column="TODATE"/>
		<property name="classtime" type="string" column="CLASSTIME"/>
		<property name="categoryids" type="string" column="CATEGORYIDS"/>
		<property name="title" type="string" column="TITLE"/>
		<property name="remark" type="string" column="REMARK"/>
		<property name="cycletype" type="string" column="CYCLETYPE"/>
	</class>
</hibernate-mapping>
