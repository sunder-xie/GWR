<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.api.ApiUserBusiness" table="API_USER_BUSINESS" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="assigned" />
		</id>
		<property name="showModel" type="string" column="SHOWMODEL" />
		<property name="coopModel" type="string" column="COOPMODEL" />
		<property name="moneyto" type="string" column="MONEYTO" />
		<property name="gewaBusUser" type="string" column="GEWABUSUSER" />
		<property name="gewaTecUser" type="string" column="GEWATECUSER" />
		<property name="partnerBusUser" type="string" column="PARTNERBUSUSER" />
		<property name="partnerTecUser" type="string" column="PARTNERTECUSER" />
		<property name="onTime" type="timestamp" column="ONTIME" />
		<property name="offTime" type="timestamp" column="OFFTIME" />
		<property name="webSite" type="string" column="WEBSITE" />
		<property name="remark" type="string" column="REMARK" />
	</class>
</hibernate-mapping>
