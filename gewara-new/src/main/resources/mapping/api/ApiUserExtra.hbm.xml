<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.api.ApiUserExtra" table="APIUSER_EXTRA" lazy="false">
		<cache usage="read-write" region="halfDay"/>
		<id name="id" type="long" column="RECORDID">
			<generator class="assigned" />
		</id>
		<property name="openDiscount" type="string" column="OPENDISCOUNT" />
		<property name="paymethod" type="string" column="PAYMETHOD" />
		<property name="chargemethod" type="string" column="CHARGEMETHOD" />
		<property name="specialmethod" type="string" column="SPECIALMETHOD" />
		<property name="proxyqry" type="string" column="PROXYQRY" />
		<property name="authFields" type="string" column="AUTH_FIELDS" />
		<property name="sourcemethod" type="string" column="SOURCEMETHOD" />
		<property name="otherinfo" type="string" column="OTHERINFO" />
	</class>
</hibernate-mapping>
