<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.api.CooperUser" table="COOPER_USER" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="assigned" />
		</id>
		<property name="loginname" type="string" column="LOGINNAME"/>
		<property name="loginpass" type="string" column="LOGINPASS"/>
		<property name="partnerid" type="long" column="PARTNERID"/>
		<property name="name" type="string" column="NAME"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="partnerids" type="string" column="PARTNERIDS"/>
		<property name="appsource" type="string" column="APPSOURCE"/>
		<property name="usertype" type="string" column="USERTYPE"/>
		<property name="roles" type="string" column="ROLES"/>
		<property name="origin" type="string" column="ORIGIN"/>
		
		<property name="realName" type="string" column="realname"/>
		<property name="mobile" type="string" column="mobile"/>
		
		<property name="tag" type="string" column="TAG"/>
		<property name="relatedids" type="string" column="RELATEDIDS"/>
		<property name="category" type="string" column="CATEGORY"/>
		<property name="categoryids" type="string" column="CATEGORYIDS"/>
		
	</class>
</hibernate-mapping>