<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.api.Synch" table="SYNCH" dynamic-update="true" lazy="false">
		<id name="cinemaid" type="long" column="CINEMAID" >
			<generator class="assigned" />
		</id>
		<property name="successtime" type="timestamp" column="SUCCESSTIME"/>
		<property name="synchtime" type="timestamp" column="SYNCHTIME"/>
		<property name="ticketnum" type="string" column="TICKETNUM"/>
		<property name="tag" type="string" column="TAG"/>
		<property name="barcodesuctime" type="timestamp" column="BARCODESUCTIME"/>
		<property name="barcodesyntime" type="timestamp" column="BARCODESYNTIME"/>
		
		<property name="gsuctime" type="timestamp" column="GSUCTIME"/>
		<property name="gsyntime" type="timestamp" column="GSYNTIME"/>
		
		<property name="ip" type="string" column="IP"/>
		<property name="synchkey" type="string" column="SYNCHKEY" />
		
		<property name="monitor" type="string" column="MONITOR"/>
		<property name="newsys" type="string" column="NEWSYS"/>
		
	</class>
</hibernate-mapping>
