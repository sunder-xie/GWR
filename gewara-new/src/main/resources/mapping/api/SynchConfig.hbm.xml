<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.api.SynchConfig" table="SYNCHCONFIG" dynamic-update="true" lazy="false">
		<cache usage="read-write" region="halfDay"/>
		<id name="ttype" type="string" column="TTYPE" >
			<generator class="assigned" />
		</id>
		<property name="tvalue" type="string" column="TVALUE"/>
	</class>
</hibernate-mapping>
