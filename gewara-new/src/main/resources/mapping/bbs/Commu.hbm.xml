<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.bbs.commu.Commu" table="COMMU"  lazy="false">
		<cache usage="read-write" region="oneHour"/>
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="name" type="string" column="name"/>
		<property name="adminid" type="long" column="ADMINID"/>
		<property name="subadminid" type="long" column="SUBADMINID"/>
		<property name="authority" type="string" column="AUTHORITY"/>
		<property name="info" type="string" column="INFO"/>
		<property name="relatedid" type="long" column="RELATEDID"/>
		<property name="tag" type="string" column="TAG"/>
		<property name="smallcategoryid" type="long" column="SMALLCATEGORYID" />
		<property name="smallcategory" type="string" column="SMALLCATEGORY"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="updatetime" type="timestamp" column="UPDATETIME"/>
		<property name="logo" type="string" column="LOGO"/>
		<property name="publicflag" type="string" column="PUBLICFLAG"/>
		<property name="joinemail" type="string" column="JOINEMAIL" />
		<property name="registjoin" type="string" column="REGISTJOIN"/>
		<property name="hotvalue" type="long" column="HOTVALUE" />
		<property name="commumembercount" type="long" column="COMMUMEMBERCOUNT" />
		<property name="status" type="string" column="STATUS" />
		<property name="countycode" type="string" column="COUNTYCODE" />
		<property name="indexareacode" type="string" column="INDEXAREACODE" />
		<property name="clickedtimes" type="integer" column="CLICKEDTIMES" />
		<property name="interesttag" type="string" column="INTERESTTAG" />
		<property name="checkstatus" type="string" column="CHECKSTATUS" />
		<property name="visitpermission" type="string" column="VISITPERMISSION"/>
		<property name="commubg" type="string" column="COMMUBG"/>
		<property name="citycode" type="string" column="CITYCODE"/>
		<!-- 2012 0808 -->
		<property name="ip" type="string" column="IP" />
	</class>
</hibernate-mapping>
