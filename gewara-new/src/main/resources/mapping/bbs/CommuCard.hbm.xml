<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.bbs.commu.CommuCard" table="COMMUCARD" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="commuid" type="long" column="COMMUID" />
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="company" type="string" column="COMPANY"/>
		<property name="realname" type="string" column="REALNAME"/>
		<property name="position" type="string" column="POSITION"/>
		<property name="industry" type="string" column="INDUSTRY"/>
		<property name="phone" type="string" column="PHONE"/>
		<property name="remark" type="string" column="REMARK"/>
		<property name="emailset" type="string" column="EMAILSET"/>
		<property name="messageset" type="string" column="MESSAGESET"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
	</class>
</hibernate-mapping>
