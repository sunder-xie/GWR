<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.bbs.commu.CommuManage" table="COMMU_MANAGE" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="commuid" type="long" column="COMMUID" />
		<property name="applymemberid" type="long" column="APPLYMEMBERID" />
		
		<property name="checkstatus" type="string" column="CHECKSTATUS" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="realname" type="string" column="REALNAME" />
		<property name="sex" type="integer" column="SEX" />
		<property name="idnumber" type="string" column="IDNUMBER" />
		<property name="contactphone" type="string" column="CONTACTPHONE" />
		<property name="email" type="string" column="EMAIL" />
		<property name="qq" type="string" column="QQ" />
		<property name="msn" type="string" column="MSN" />
		<property name="remark" type="string" column="REMARK" />
		<property name="alipayname" type="string" column="ALIPAYNAME" />
		<property name="alipay" type="string" column="ALIPAY" />
	</class>
</hibernate-mapping>
