<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.bbs.commu.CommuMember" table="COMMUMEMBER"  lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="memberid" column="MEMBERID" type="long"/>
		<property name="commuid" column="COMMUID" type="long"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="bbsmail" type="string" column="BBSMAIL"/>
		<property name="adminmail" type="string" column="ADMINMAIL"/>
		<property name="flag" type="string" column="FLAG"/>
	</class>
</hibernate-mapping>
