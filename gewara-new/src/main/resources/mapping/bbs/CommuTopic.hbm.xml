<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.bbs.commu.CommuTopic" table="COMMU_TOPIC" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="commuid" type="long" column="COMMUID" />
		<property name="topicname" type="string" column="TOPICNAME"/>
		<property name="ordernum" type="integer" column="ORDERNUM"/>
		<property name="displaynum" type="integer" column="DISPLAYNUM"/>
	</class>
</hibernate-mapping>
