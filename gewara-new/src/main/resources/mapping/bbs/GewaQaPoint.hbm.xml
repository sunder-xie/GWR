<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.bbs.qa.GewaQaPoint" table="GEWAQAPOINT" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
	 	<property name="memberid" type="long" column="MEMBERID" />
		<property name="point" type="integer" column="POINT" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="answerid" type="long" column="ANSWERID" />
		<property name="questionid" type="long" column="QUESTIONID" />
		<property name="tag" type="string" column="TAG" />
	</class>
</hibernate-mapping>
