<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.bbs.MemberMark" table="MEMBERMARK"  lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="tag" type="string" column="TAG"/>
		<property name="relatedid" type="long" column="RELATEDID"/>
		<property name="markname" type="string" column="MARKNAME"/>
		<property name="markvalue" type="integer" column="MARKVALUE"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="memberid" type="long" column="MEMBERID" />
		<property name="nickname" type="string" column="NICKNAME" />
		<property name="flag" type="string" column="FLAG" />
	</class>
</hibernate-mapping>
