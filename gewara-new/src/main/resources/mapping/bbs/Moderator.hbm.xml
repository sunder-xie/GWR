<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.bbs.Moderator" table="MODERATOR" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="memberid" type="string" column="MEMBERID"/>
		<property name="title" type="string" column="TITLE"/>
		<property name="summary" type="string" column="SUMMARY"/>
		<property name="ordernum" type="integer" column="ORDERNUM"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="type" type="string" column="TYPE"/>
		<property name="showfloor" type="integer" column="SHOWFLOOR"/>
		<property name="showaddress" type="integer" column="SHOWADDRESS"/>
		<property name="mstatus" type="string" column="MSTATUS"/>
		<property name="commentcount" type="integer" column="COMMENTCOUNT"/>
	</class>
</hibernate-mapping>
