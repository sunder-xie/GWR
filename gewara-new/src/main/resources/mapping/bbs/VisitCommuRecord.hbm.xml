<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.bbs.commu.VisitCommuRecord" table="VISITCOMMURECORD"  lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="commuid" type="long" column="COMMUID"/>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="visitnum" type="integer" column="VISITNUM"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="lasttime" type="timestamp" column="LASTTIME"/>
	</class>
</hibernate-mapping>
