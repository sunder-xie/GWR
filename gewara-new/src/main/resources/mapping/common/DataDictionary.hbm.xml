<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.common.DataDictionary" table="DATA_DICTIONARY" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="objectName" type="string" column="OBJECT_NAME"/>
		<property name="propertyName" type="string" column="PROPERTY_NAME"/>
		<property name="propertyRealName" type="string" column="PROPERTY_REALNAME"/>
		<property name="dataType" type="string" column="DATA_TYPE"/>
		<property name="propertyLength" type="integer" column="PROPERTY_LENGTH"/>
		<property name="required" type="string" column="required"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="updatetime" type="timestamp" column="UPDATETIME"/>
	</class>
</hibernate-mapping>
