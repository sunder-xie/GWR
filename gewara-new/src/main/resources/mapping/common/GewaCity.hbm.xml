<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.common.GewaCity" table="GEWA_CITY" lazy="false">
		<id name="citycode" type="string" column="CITYCODE">
			<generator class="assigned" />
		</id>
		<property name="provincecode" type="string" column="PROVINCECODE" />
		<property name="provincename" type="string" column="PROVINCENAME" />
		<property name="cityname" type="string" column="CITYNAME"/>
		
		<property name="showHot" type="string" column="SHOWHOT"/>
		<property name="showIdx" type="string" column="SHOWIDX"/>
		<property name="showAdm" type="string" column="SHOWADM"/>
		
		<property name="citySort" type="integer" column="CITYSORT"/>
		<property name="provinceSort" type="integer" column="PROVINCESORT"/>
		
		<property name="pinyin" type="string" column="PINYIN"/>
		<property name="py" type="string" column="PY"/>
		
		<property name="manmethod" type="string" column="MANMETHOD"/>
		<property name="bpointx" type="string" column="BPOINTX"/>
		<property name="bpointy" type="string" column="BPOINTY"/>
		<property name="serviceType" type="string" column="SERVICE_TYPE"/>
	</class>
</hibernate-mapping>
