<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.common.HisData" table="HIS_DATA" lazy="false">
		<id name="key" type="string" column="dkey" >
			<generator class="assigned" />
		</id>
		<property name="jsonData" type="string" column="JSONDATA" />
		<property name="validtime" type="timestamp" column="VALIDTIME"/>
	</class>
</hibernate-mapping>