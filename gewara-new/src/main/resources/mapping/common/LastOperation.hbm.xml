<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.common.LastOperation" table="LAST_OPERATION" lazy="false">
		<id name="lastkey" type="string" column="LASTKEY">
			<generator class="assigned" />
		</id>
		<property name="lastvalue" type="string" column="LASTVALUE"/>
		<property name="tag" type="string" column="TAG"/>
		<property name="lasttime" type="timestamp" column="LASTTIME" />
		<property name="validtime" type="timestamp" column="VALIDTIME" />
	</class>
</hibernate-mapping>
