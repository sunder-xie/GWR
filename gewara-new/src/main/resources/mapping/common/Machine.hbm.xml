<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.machine.Machine" table="GEWAMACHINE" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" /> 
		</id>
		<property name="machinenumber" type="string" column="MACHINENUMBER" />
		<property name="machinename" type="string" column="MACHINENAME" />
		<property name="cinemaid" type="long" column="CINEMAID" />
		<property name="hfhopendate" type="date" column="HFHOPENDATE" />
		<property name="leavedate" type="date" column="LEAVEDATE" />
		<property name="buydate" type="date" column="BUYDATE" />
		<property name="linkmethod" type="string" column="LINKMETHOD" />
		<property name="touchtype" type="string" column="TOUCHTYPE" />
		<property name="ticketcount" type="integer" column="TICKETCOUNT" />
		<property name="machinetype" type="string" column="MACHINETYPE" />
		<property name="machinestatus" type="string" column="MACHINESTATUS" />
		<property name="machineservice" type="string" column="MACHINESERVICE" />
		<property name="machinecontent" type="string" column="MACHINECONTENT" />
		<property name="machineowner" type="string" column="MACHINEOWNER" />
		<property name="machineusage" type="string" column="MACHINEUSAGE" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="usedate" type="date" column="USEDATE" />
		<property name="remark" type="string" column="REMARK" />
		<property name="ip" type="string" column="IP" />
		<property name="ipremark" type="string" column="IPREMARK" />
		<property name="operMember" type="string" column="OPERMEMBER" />
		<property name="updatetime" type="timestamp" column="UPDATETIME"/>
		<property name="citycode" type="string" column="CITYCODE" />
		
	</class>
</hibernate-mapping>
