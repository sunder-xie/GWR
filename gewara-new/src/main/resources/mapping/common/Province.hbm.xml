<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.common.Province" table="PROVINCE" mutable="false" lazy="false">
		<cache usage="read-only" region="eternal"/>
		<id name="provincecode" type="string" column="PROVINCECODE">
			<generator class="assigned" />
		</id>
		<property name="provincename" type="string" column="PROVINCENAME" />
		<property name="provinceename" type="string" column="PROVINCEENAME" />
		<property name="nationcode" type="string" column="NATIONCODE" />
		<property name="nationname" type="string" column="NATIONNAME" />
		<property name="nationename" type="string" column="NATIONENAME" />
		<property name="continentcode" type="string" column="CONTINENTCODE" />
		<property name="continentname" type="string" column="CONTINENTNAME" />
		<property name="continentename" type="string" column="CONTINENTENAME" />
		<property name="regionlevel" type="string" column="REGIONLEVEL" />
	</class>
</hibernate-mapping>
