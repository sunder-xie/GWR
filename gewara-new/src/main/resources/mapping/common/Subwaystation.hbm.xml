<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.common.Subwaystation" table="SUBWAYSTATION" mutable="false" lazy="false">
		<cache usage="read-write" region="eternal"/>
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="stationname" type="string" column="STATIONNAME"/>
		<property name="citycode" type="string" column="CITYCODE"/>
	</class>
</hibernate-mapping>
