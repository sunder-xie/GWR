<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.common.UploadPic" table="UPLOAD_PIC" lazy="false">
		<id name="picname" type="string" column="PICNAME">
			<generator class="assigned" />
		</id>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="modifytime" type="long" column="MODIFYTIME"/>
		<property name="picsize" type="integer" column="PICSIZE"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="tag" type="string" column="TAG"/>
		<property name="relatedid" type="long" column="RELATEDID"/>
	</class>
</hibernate-mapping>
