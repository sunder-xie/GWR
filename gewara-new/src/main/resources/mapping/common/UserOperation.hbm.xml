<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.common.UserOperation" table="USER_OPERATION" lazy="false">
		<id name="opkey" type="string" column="OPKEY">
			<generator class="assigned" />
		</id>
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="updatetime" type="timestamp" column="UPDATETIME" />
		<property name="validtime" type="timestamp" column="VALIDTIME" />
		<property name="opnum" type="integer" column="OPNUM"/>
		<property name="refused" type="integer" column="REFUSED"/>
		<property name="tag" type="string" column="TAG"/>
		<property name="secondkey" type="string" column="SECONDKEY"/>
	</class>
</hibernate-mapping>
