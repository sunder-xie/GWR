<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.content.PhoneAdvertisement" table="phoneadvertisement">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="advlink" type="string" column="advlink"/>
		<property name="link" type="string" column="link"/>
		<property name="title" type="string" column="title"/>
		<property name="apptype" type="string" column="apptype"/>
		<property name="osType" type="string" column="osType"/>
		<property name="citycode" type="string" column="citycode"/>
		<property name="addtime" type="timestamp" column="addtime"/>
		<property name="isshow" type="string" column="isshow"/>
		<property name="status" type="string" column="status"/>
		<property name="starttime" type="timestamp" column="starttime"/>
		<property name="endtime" type="timestamp" column="endtime"/>
		<property name="relatedid" type="long" column="relatedid"/>
		<property name="activityid" type="long" column="activityid"/>
		<property name="summary" type="string" column="summary"/>
		
		<property name="rank" type="integer" column="rank"/>
		<property name="advertType" type="string" column="adverttype"/>
		
		<property name="sharefriend" type="string" column="sharefriend"/>
		
	</class>
</hibernate-mapping>
