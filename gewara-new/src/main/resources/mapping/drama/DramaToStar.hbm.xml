<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.drama.DramaToStar" table="DRAMA_TO_STAR"  dynamic-update="true" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="dramaid" type="long" column="DRAMAID"/>
		<property name="starid" type="long" column="STARID"/>
		<property name="numsort" type="integer" column="NUMSORT"/>
		<property name="tag" type="string" column="TAG"/>
	</class>
</hibernate-mapping>
