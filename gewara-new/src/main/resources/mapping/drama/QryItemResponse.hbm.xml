<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.drama.QryItemResponse" table="QRY_ITEM_RESPONSE" lazy="false">
		<cache usage="read-write" region="halfDay"/>
		<id name="resid" type="string" column="RESID">
			<generator class="assigned" />
		</id>
		<property name="updatetime" type="timestamp" column="UPDATETIME"/>
		<property name="response" type="string" column="RESPONSE"/>
	</class>
</hibernate-mapping>