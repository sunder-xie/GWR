<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.drama.TheatreSeatArea" table="THEATRE_SEAT_AREA" dynamic-update="true" lazy="false">
		<cache usage="read-write" region="halfDay"/>
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="dpid" type="long" column="DPID" />
		<property name="theatreid" type="long" column="THEATREID" />
		<property name="dramaid" type="long" column="DRAMAID" />
		<property name="areaname" type="string" column="AREANAME" />
		<property name="enname" type="string" column="ENNAME" />
		<property name="seller" type="string" column="SELLER" />
		<property name="sellerseq" type="string" column="SELLERSEQ" />
		<property name="fieldnum" type="string" column="FIELDNUM" />
		<property name="roomnum" type="string" column="ROOMNUM" />
		<property name="description" type="string" column="DESCRIPTION" />
		<property name="standing" type="string" column="STANDING" />
		<property name="total" type="integer" column="TOTAL" />
		<property name="limitnum" type="integer" column="LIMITNUM" />
		<property name="firstline" type="integer" column="FIRSTLINE" />
		<property name="firstrank" type="integer" column="FIRSTRANK" />
		<property name="linenum" type="integer" column="LINENUM" />
		<property name="ranknum" type="integer" column="RANKNUM" />
		<property name="hotzone" type="string" column="HOTZONE" />
		<property name="mobilehotzone" type="string" column="MOBILEHOTZONE" />
		<property name="seatmap" type="string" column="SEATMAP" />
		<property name="status" type="string" column="STATUS" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="updatetime" type="timestamp" column="UPDATETIME" />
		<property name="gsellnum" type="integer" column="GSELLNUM" />
		<property name="csellnum" type="integer" column="CSELLNUM" />
		<property name="locknum" type="integer" column="LOCKNUM" />
		<property name="otherinfo" type="string" column="OTHERINFO" />
	</class>
</hibernate-mapping>
