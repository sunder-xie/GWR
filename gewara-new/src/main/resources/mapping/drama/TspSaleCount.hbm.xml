<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.drama.TspSaleCount" table="TSP_SALE_COUNT" dynamic-update="true" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence">
				<param name="sequence">tsp_sale_count_seq</param>
			</generator>
		</id>
		<property name="dpid" type="long" column="DPID" />
		<property name="priceid" type="long" column="PRICEID" />
		<property name="sales" type="integer" column="SALES" />
		<property name="orderid" type="long" column="ORDERID" />
		<property name="status" type="string" column="STATUS" />
		<property name="ptype" type="string" column="PTYPE" />
		<property name="validtime" type="timestamp" column="VALIDTIME" />
	</class>
</hibernate-mapping>
