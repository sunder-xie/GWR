<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.draw.WinnerInfo" table="WINNERINFO" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="activityid" type="long" column="ACTIVITYID"/>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="nickname" type="string" column="NICKNAME"/>
		<property name="prizeid" type="long" column="PRIZEID"/>
		<property name="mobile" type="string" column="MOBILE"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="tag" type="string" column="TAG"/>
		<property name="relatedid" type="long" column="RELATEDID"/>
		<property name="remark" type="string" column="REMARK"/>
		<property name="ip" type="string" column="IP"/>
	</class>
</hibernate-mapping>
