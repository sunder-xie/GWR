<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.express.ExpressOrder" table="express_order" dynamic-update="true" lazy="false">
		<id name="id" type="string" column="RECORDID">
			<generator class="assigned" />
		</id>
		<property name="expressnote" type="string" column="EXPRESSNOTE" />
		<property name="expressType" type="string" column="EXPRESSTYPE" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="updatetime" type="timestamp" column="UPDATETIME" />
		<property name="status" type="string" column="STATUS" />
		<property name="otherinfo" type="string" column="OTHERINFO" />
	</class>
</hibernate-mapping>
