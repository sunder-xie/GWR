<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.machine.Barcode" table="BARCODE" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" /> 
		</id>
		<property name="barcode" type="string" column="BARCODE" />
		<property name="tradeno" type="string" column="TRADENO" />
		<property name="status" type="string" column="STATUS" />
		<property name="relatedid" type="long" column="RELATEDID" />
		<property name="placeid" type="long" column="PLACEID" />
		<property name="itemid" type="long" column="ITEMID" />
		<property name="flag" type="string" column="FLAG" />
		<property name="serialno" type="string" column="SERIALNO" />
		<property name="validtime" type="timestamp" column="VALIDTIME"/>
		<property name="taketime" type="timestamp" column="TAKETIME"/>
		<property name="updatetime" type="timestamp" column="UPDATETIME" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		
	</class>
</hibernate-mapping>
