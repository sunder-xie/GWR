<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.machine.MachineSynch" table="MACHINE_SYNCH" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" /> 
		</id>
		<property name="placeid" type="long" column="PLACEID" />
		<property name="tag" type="string" column="TAG" />
		<property name="macid" type="string" column="MACID" />
		<property name="successtime" type="timestamp" column="SUCCESSTIME"/>
		<property name="synchtime" type="timestamp" column="SYNCHTIME"/>
		
	</class>
</hibernate-mapping>
