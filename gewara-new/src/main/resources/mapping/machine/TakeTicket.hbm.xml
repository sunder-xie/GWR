<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.machine.TakeTicket" table="TAKETICKET" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" /> 
		</id>
		<property name="placeid" type="long" column="PLACEID" />
		<property name="macid" type="string" column="MACID" />
		<property name="tradeno" type="string" column="TRADENO"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="taketime" type="timestamp" column="TAKETIME"/>
		
	</class>
</hibernate-mapping>
