<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.mobile.AsConfig" table="AS_CONFIG" lazy="false">
		<cache usage="read-write" region="halfDay"/>
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
	 	<property name="partnerid" type="long" column="PARTNERID" />
	 	<property name="appsource" type="string" column="APPSOURCE" />
	 	<property name="paymethod" type="string" column="PAYMETHOD" />
	 	<property name="specialmethod" type="string" column="SPECIALMETHOD" />
	 	<property name="appVersion" type="string" column="APP_VERSION" />
	</class>
</hibernate-mapping>
