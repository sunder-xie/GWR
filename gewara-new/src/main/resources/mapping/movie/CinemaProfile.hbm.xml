<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.movie.CinemaProfile" table="CINEMAPROFILE" lazy="false">
		<cache usage="read-write" region="halfDay"/>
		<id name="id" type="long" column="CINEMAID">
			<generator class="assigned" />
		</id>
		<property name="takemethod" type="string" column="TAKEMETHOD"/>
		<property name="notifymsg1" type="string" column="NOTIFYMSG1"/>
		<property name="notifymsg2" type="string" column="NOTIFYMSG2"/>
		<property name="opentime" type="string" column="OPENTIME"/>
		<property name="closetime" type="string" column="CLOSETIME"/>
		<property name="startsale" type="string" column="STARTSALE"/>
		<property name="endsale" type="string" column="ENDSALE"/>
		<property name="topicid" type="long" column="TOPICID"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="popcorn" type="string" column="POPCORN"/>
		<property name="servicefee" type="string" column="SERVICEFEE"/>
		<property name="cminute" type="integer" column="CMINUTE" />
		<property name="fee" type="integer" column="FEE" />
		<property name="isRefund" type="string" column="isRefund" />
		<property name="opentype" type="string" column="OPENTYPE" />
		<property name="direct" type="string" column="DIRECT" />
		<property name="prompting" type="string" column="PROMPTING" />
		<property name="openDay" type="integer" column="OPENDAY" />
		<property name="openDayTime" type="string" column="OPENDAYTIME" />
		<property name="takeAddress" type="string" column="TAKE_ADDRESS" />
	</class>
</hibernate-mapping>
