<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.movie.MovieTierPrice" table="MOVIE_TIERPRICE" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="movieid" type="long" column="MOVIEID" />
		<property name="type" type="string" column="TYPE" />
		<property name="price" type="integer" column="PRICE" />
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="edition3D" type="integer" column="EDITION_3D"/>
		<property name="editionJumu" type="integer" column="EDITION_JUMU"/>
		<property name="editionIMAX" type="integer" column="EDITION_IMAX"/>
		
		<property name="rangeEdition3D" type="integer" column="RANGE_EDITION_3D"/>
		<property name="rangePrice" type="integer" column="RANGE_PRICE"/>
		<property name="rangeEditionJumu" type="integer" column="RANGE_EDITION_JUMU"/>
		<property name="rangeEditionIMAX" type="integer" column="RANGE_EDITION_IMAX"/>
		<property name="startTime" type="timestamp" column="STARTTIME"/>
		<property name="endTime" type="timestamp" column="ENDTIME"/>
	</class>
</hibernate-mapping>
