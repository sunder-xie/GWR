<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.movie.MultiPlayItem" table="MULTIPLAYITEM" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="movieid" type="long" column="MOVIEID" />
		<property name="multiPlayid" type="long" column="MULTIPLAYID" />
		<property name="language" type="string" column="LANGUAGE" />
		<property name="edition" type="string" column="EDITION"/>
		<property name="playorder" type="integer" column="PLAYORDER"/>
		
	</class>
</hibernate-mapping>
