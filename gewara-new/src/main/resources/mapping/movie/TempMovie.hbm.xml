<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.movie.TempMovie" table="TEMPMOVIE" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="moviename" type="string" column="MOVIENAME"/>
		<property name="type" type="string" column="TYPE"/>
		<property name="content" type="string" column="CONTENT"/>
		<property name="state" type="string" column="STATE"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="point" type="integer" column="POINT"/>
		<property name="reason" type="string" column="REASON" />
		<property name="actors" type="string" column="ACTORS" />
		<property name="director" type="string" column="DIRECTOR" />
		<property name="logo" type="string" column="LOGO" />
		<property name="releaseDate" type="timestamp" column="RELEASE_DATE" />
	</class>
</hibernate-mapping>
