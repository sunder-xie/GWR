<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.partner.CallbackOrder" table="CALLBACK_ORDER" lazy="false">
		<id name="orderid" type="long" column="ORDERID">
			<generator class="assigned" />
		</id>
		<property name="partnerid" type="long" column="PARTNERID" />
		<property name="status" type="string" column="STATUS"/>
		<property name="calltimes" type="integer" column="CALLTIMES"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="updatetime" type="timestamp" column="UPDATETIME"/>
	</class>
</hibernate-mapping>
