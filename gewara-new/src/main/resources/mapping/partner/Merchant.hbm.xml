<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.partner.Merchant" table="MERCHANT" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence"><param name="sequence">APIUSERSEQ</param></generator>
		</id>
		<property name="loginname" type="string" column="LOGINNAME"/>
		<property name="loginpass" type="string" column="LOGINPASS"/>
		<property name="mername" type="string" column="MERNAME"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="roles" type="string" column="ROLES"/>
		<property name="company" type="string" column="COMPANY"/>
		<property name="opentype" type="string" column="OPENTYPE"/>
		<property name="relatelist" type="string" column="RELATELIST" />
		<property name="contact" type="string" column="CONTACT"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
	</class>
</hibernate-mapping>