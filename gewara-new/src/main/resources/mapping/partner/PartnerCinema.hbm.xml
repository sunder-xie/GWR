<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.partner.PartnerCinema" table="PARTNER_CINEMA" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="partnerid" type="long" column="PARTNERID"/>
		<property name="cinemaid" type="long" column="CINEMAID"/>
		<property name="pcid" type="string" column="PCID"/>
		<property name="pcname" type="string" column="PCNAME"/>
		<property name="cityname" type="string" column="CITYNAME"/>
	</class>
</hibernate-mapping>
