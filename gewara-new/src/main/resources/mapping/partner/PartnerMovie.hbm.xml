<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.partner.PartnerMovie" table="PARTNER_MOVIE" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="partnerid" type="long" column="PARTNERID"/>
		<property name="movieid" type="long" column="MOVIEID"/>
		<property name="pmid" type="string" column="PMID"/>
		<property name="pmname" type="string" column="PMNAME"/>
	</class>
</hibernate-mapping>
