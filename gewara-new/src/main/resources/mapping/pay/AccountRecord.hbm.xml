<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.AccountRecord" table="ACCOUNT_RECORD" lazy="false">
		<id name="accountid" type="long" column="ACCOUNTID" />
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="membername" type="string" column="MEMBERNAME"/>
		<property name="abanlance" type="integer" column="ABANLANCE" />
		<property name="lbanlance" type="integer" column="LBANLANCE" />
		<property name="charge" type="integer" column="CHARGE" />
		<property name="gewapay" type="integer" column="GEWAPAY" />
		<property name="refund" type="integer" column="REFUND" />
		<property name="lasttime" type="timestamp" column="lasttime" />
		<property name="updatetime" type="timestamp" column="updatetime" />
		<property name="status" type="string" column="STATUS"/>
	</class>
</hibernate-mapping>
