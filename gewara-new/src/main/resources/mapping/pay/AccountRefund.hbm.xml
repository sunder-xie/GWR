<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.AccountRefund" table="ACCOUNT_REFUND" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<version name="version" column="REFUND_VERSION" type="integer"/>
		<property name="tradeno" type="string" column="TRADENO" />
		<property name="origin" type="string" column="ORIGIN" />
		<property name="reason" type="string" column="REASON" />
		<property name="status" type="string" column="STATUS" />
		<property name="paymethod" type="string" column="PAYMETHOD" />
		<property name="memberid" type="long" column="MEMBERID" />
		<property name="partnerid" type="long" column="PARTNERID" />
		<property name="mobile" type="string" column="MOBILE" />
		<property name="dealtime" type="timestamp" column="DEALTIME" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="remark" type="string" column="REMARK" />
		<property name="amount" type="integer" column="AMOUNT" />
		<property name="applyuser" type="long" column="APPLYUSER" />
		<property name="dealuser" type="long" column="DEALUSER" />
	</class>
</hibernate-mapping>
