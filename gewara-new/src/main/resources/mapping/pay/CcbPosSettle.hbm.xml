<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.CcbPosSettle" table="CCBPOS_SETTLE" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="cardpan" type="string" column="CARDPAN"/>
		<property name="tradeno" type="string" column="TRADENO"/>
		<property name="amount" type="integer" column="AMOUNT" />
		<property name="handfee" type="integer" column="HANDFEE" />
		<property name="preno" type="string" column="PRENO"/>
		
		<property name="settledate" type="date" column="SETTLEDATE" />
		<property name="paiddate" type="date" column="PAIDDATE" />
		<property name="alipaid" type="integer" column="ALIPAID" />
		<property name="payseqno" type="string" column="PAYSEQNO" />
		<property name="settle" type="string" column="SETTLE" />
	</class>
</hibernate-mapping>
