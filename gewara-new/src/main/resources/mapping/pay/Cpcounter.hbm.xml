<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.Cpcounter" table="CPCOUNTER" lazy="false" dynamic-update="true">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<version name="version" column="DATA_VERSION" type="integer"/>
		<property name="spcounterid" type="long" column="SPCOUNTERID"/>
		<property name="flag" type="string" column="FLAG"/>
		<property name="cpcode" type="string" column="CPCODE" />
		<property name="basenum" type="integer" column="BASENUM"/>
		<property name="allownum" type="integer" column="ALLOWNUM"/>
		<property name="limitnum" type="integer" column="LIMITNUM"/>
		<property name="sellquantity" type="integer" column="SELLQUANTITY"/>
		<property name="sellorder" type="integer" column="SELLORDER"/>
		<property name="allquantity" type="integer" column="ALLQUANTITY"/>
		<property name="allordernum" type="integer" column="ALLORDERNUM"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="updatetime" type="timestamp" column="UPDATETIME"/>
	</class>
</hibernate-mapping>
