<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.DealExpressOrder" table="DEAL_EXPRESS_ORDER" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="orderid" type="long" column="ORDERID"/>
		<property name="tradeNo" type="string" column="TRADENO"/>
		<property name="dealuser" type="long" column="DEALUSER"/>
		<property name="nickname" type="string" column="NICKNAME"/>
		<property name="mobile" type="string" column="MOBILE"/>
		<property name="dealtype" type="string" column="DEALTYPE"/>
		<property name="dealStatus" type="string" column="DEALSTATUS"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="remark" type="string" column="REMARK"/>
	</class>
</hibernate-mapping>
