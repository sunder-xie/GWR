<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.ElecCardBatch" table="ELECCARD_BATCH" lazy="false">
		<cache usage="read-write" region="halfDay"/>
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="pid" type="long" column="PID" />
		<property name="tag" type="string" column="TAG" />
	 	<property name="cardtype" type="string" column="CARDTYPE" />
	 	<property name="amount" type="integer" column="PRICE" />
		<property name="timefrom" type="timestamp" column="TIMEFROM" />
		<property name="timeto" type="timestamp" column="TIMETO" />
		<property name="remark" type="string" column="REMARK" />
		<property name="addtime1" type="string" column="ADDTIME1"/>
		<property name="addtime2" type="string" column="ADDTIME2"/>
		<property name="addweek" type="string" column="ADDWEEK"/>
		<property name="opentime" type="string" column="OPENTIME" />
		<property name="closetime" type="string" column="CLOSETIME" />
		<property name="weektype" type="string" column="WEEKTYPE" />
		<property name="validcinema" type="string" column="VALIDCINEMA" />
		<property name="validmovie" type="string" column="VALIDMOVIE" />
		<property name="validitem" type="string" column="VALIDITEM" />
		<property name="validprice" type="string" column="VALIDPRICE" />
		<property name="validpartner" type="string" column="VALIDPARTNER" />
		<property name="notifymsg" type="string" column="NOTIFYMSG" />
		<property name="daynum" type="integer" column="DAYNUM" />
		<property name="bindpay" type="string" column="BINDPAY" />
		<property name="citycode" type="string" column="CITYCODE" />
		<property name="citypattern" type="string" column="CITYPATTERN" />
		<property name="bindgoods" type="long" column="BINDGOODS" />
		<property name="bindratio" type="integer" column="BINDRATIO" />
		<property name="costtype" type="string" column="COSTTYPE" />
		<property name="costnum" type="integer" column="COSTNUM" />
		<property name="costnum3D" type="integer" column="COSTNUM3D" />
		<property name="edition" type="string" column="EDITION" />
		<property name="limitdesc" type="string" column="LIMITDESC" />
		<property name="activation" type="string" column="ACTIVATION" />
		<property name="exchangetype" type="string" column="EXCHANGETYPE" />
		<property name="channelinfo" type="string" column="CHANNELINFO" />
		<property name="appoint" type="integer" column="APPOINT" />
		<property name="soldType" type="string" column="SOLD_TYPE" />
		<property name="delayDays" type="integer" column="DELAY_DAYS" />
		<property name="delayUseDays" type="integer" column="DELAY_USE_DAYS" />
		<property name="delayFee" type="integer" column="DELAY_FEE" />
	</class>
</hibernate-mapping>
