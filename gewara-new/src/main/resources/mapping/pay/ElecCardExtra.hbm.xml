<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.ElecCardExtra" table="ELECCARD_EXTRA" lazy="false">
		<id name="batchid" type="long" column="BATCHID">
			<generator class="assigned" />
		</id>
		<property name="pid" type="long" column="PID" />
		<property name="channel" type="string" column="CHANNEL" />
		<property name="issuerid" type="long" column="ISSUERID" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="adduserid" type="long" column="ADDUSERID" />
		<property name="soldtime" type="timestamp" column="SOLDTIME" />
		<property name="sellerid" type="long" column="SELLERID" />
		<property name="sellprice" type="integer" column="SELLPRICE" />
		<property name="category1" type="string" column="CATEGORY1" />
		<property name="category2" type="string" column="CATEGORY2" />
		<property name="applycity" type="string" column="APPLYCITY" />
		<property name="applydept" type="string" column="APPLYDEPT" />
		<property name="applytype" type="string" column="APPLYTYPE" />
		<property name="merchantid" type="long" column="MERCHANTID" />
		<property name="cardcount" type="integer" column="CARDCOUNT" />
		<property name="usedcount" type="integer" column="USEDCOUNT" />
		<property name="delcount" type="integer" column="DELCOUNT" />
		<property name="newcount" type="integer" column="NEWCOUNT" />
		<property name="soldcount" type="integer" column="SOLDCOUNT" />
		<property name="lockcount" type="integer" column="LOCKCOUNT" />
		<property name="issuecount" type="integer" column="ISSUECOUNT" />
		<property name="mincardno" type="string" column="MINCARDNO" />
		<property name="maxcardno" type="string" column="MAXCARDNO" />
		<property name="statstime" type="timestamp" column="STATSTIME" />
		<property name="status" type="string" column="STATUS" />
	</class>
</hibernate-mapping>
