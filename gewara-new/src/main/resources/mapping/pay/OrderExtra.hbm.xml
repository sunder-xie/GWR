<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.OrderExtra" table="ORDER_EXTRA" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="assigned" />
		</id>
		<property name="tradeno" type="string" column="TRADENO" />
		<property name="status" type="string" column="STATUS" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="updatetime" type="timestamp" column="UPDATETIME" />
		<property name="invoice" type="string" column="INVOICE" />
		<property name="pretype" type="string" column="PRETYPE" />
		<property name="memberid" type="long" column="MEMBERID" />
		<property name="partnerid" type="long" column="PARTNERID" />
		<property name="ordertype" type="string" column="ORDERTYPE" />
		<property name="expressnote" type="string" column="EXPRESSNOTE" />
		<property name="expresstype" type="string" column="EXPRESSTYPE" />
		<property name="processLevel" type="string" column="PROCESSLEVEL" />
		<property name="expressStatus" type="string" column="EXPRESSSTATUS" />
		<property name="dealStatus" type="string" column="DEALSTATUS" />
		<property name="dealUser" type="long" column="DEALUSER" />
	</class>
</hibernate-mapping>
