<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.OrderNote" table="ORDER_NOTE" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="orderid" type="long" column="ORDERID" />
		<property name="tradeno" type="string" column="TRADENO" />
		<property name="ordertype" type="string" column="ORDERTYPE" />
		<property name="mobile" type="string" column="MOBILE" />
		<property name="placename" type="string" column="PLACENAME" />
		<property name="placetype" type="string" column="PLACETYPE" />
		<property name="placeid" type="long" column="PLACEID" />
		<property name="itemname" type="string" column="ITEMNAME" />
		<property name="itemtype" type="string" column="ITEMTYPE" />
		<property name="itemid" type="long" column="ITEMID" />
		<property name="checkpass" type="string" column="CHECKPASS" />
		<property name="ticketnum" type="integer" column="TICKETNUM" />
		<property name="smallitemtype" type="string" column="SMALLITEMTYPE" />
		<property name="smallitemid" type="long" column="SMALLITEMID" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="updatetime" type="timestamp" column="UPDATETIME" />
		<property name="playtime" type="timestamp" column="PLAYTIME" />
		<property name="taketime" type="timestamp" column="TAKETIME" />
		<property name="validtime" type="timestamp" column="VALIDTIME" />
		<property name="modifytime" type="timestamp" column="MODIFYTIME" />
		<property name="message" type="string" column="MESSAGE" />
		<property name="result" type="string" column="RESULT" />
		<property name="serialno" type="string" column="SERIALNO" />
		<property name="status" type="string" column="STATUS" />
		<property name="description" type="string" column="DESCRIPTION" />
		<property name="fromup" type="string" column="FROMUP" />
		<property name="express" type="string" column="EXPRESS" />
		<property name="otherinfo" type="string" column="otherinfo" />
	</class>
</hibernate-mapping>
