<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.OtherFeeDetail" table="OTHERFEE_DETAIL" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="orderid" type="long" column="ORDERID"/>
		<property name="reason" type="string" column="REASON"/>
		<property name="fee" type="integer" column="FEE" />
		<property name="quantity" type="integer" column="QUANTITY" />
		<property name="feetype" type="string" column="FEETYPE" />
	</class>
</hibernate-mapping>
