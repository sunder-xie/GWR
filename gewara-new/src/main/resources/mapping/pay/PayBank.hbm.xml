<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.PayBank" table="PAYBANK" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="name" type="string" column="NAME" />
		<property name="cnname" type="string" column="CNNAME" />
		<property name="citycode" type="string" column="CITYCODE" />
		<property name="paymethod" type="string" column="PAYMETHOD" />
		<property name="banktype" type="string" column="BANKTYPE" />
		<property name="sortnum" type="integer" column="SORTNUM" />
	</class>
</hibernate-mapping>
