<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.PayCityMerchant" table="pay_city_merchant" dynamic-update="true" optimistic-lock="version" lazy="false">
		<id name="id" type="long" column="recordid">
			<generator class="sequence" />
		</id>
		<property name="gatewayId" type="long" column="gateway_id" />		
		<property name="areaCode" type="string" column="area_code" />
		<property name="areaType" type="string" column="area_type" />
		<property name="merchantCode" type="string" column="merchant_code" />
		<property name="modifyTime" type="timestamp" column="modify_time" />
		<property name="modifyUser" type="string" column="modify_user" />
	</class>
</hibernate-mapping>
