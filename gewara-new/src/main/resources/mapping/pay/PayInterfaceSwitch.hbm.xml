<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.PayInterfaceSwitch" table="pay_interface_switch" dynamic-update="true" optimistic-lock="version" lazy="false">
		<id name="gatewayCode" column="gateway_code" type="string">
			<generator class="assigned"/>
		</id>
		<property name="interfaceType" type="string" column="interface_type" />
		<property name="addTime" type="timestamp" column="add_time" />
		<property name="modifyTime" type="timestamp" column="modify_time" />
		<property name="modifyUser" type="string" column="modify_user" />
	</class>
</hibernate-mapping>
