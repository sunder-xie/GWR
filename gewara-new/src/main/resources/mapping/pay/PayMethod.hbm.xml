<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.PayMethod" table="PAY_METHOD" lazy="false">
		<id name="payMethod" type="string" column="PAYMETHOD">
			<generator class="assigned"/>
		</id>
		<property name="payMethodText" type="string" column="PAYMETHOD_TEXT"/>
		<property name="tag" type="string" column="FLAG"/>
		<property name="mangerUrl" type="string" column="MANGERURL"/>
	</class>
</hibernate-mapping>
