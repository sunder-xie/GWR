<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.PubMember" table="PUBMEMBER" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="pubid" type="long" column="PUBID"/>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="price" type="integer" column="PRICE"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="pointvalue" type="integer" column="POINTVALUE"/>
	</class>
</hibernate-mapping>
