<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.PubSale" table="PUBSALE" dynamic-update="true" optimistic-lock="version" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<version name="version" type="integer" column="VERSION"/>
		<property name="name" type="string" column="NAME"/>
		<property name="lowerprice" type="integer" column="LOWERPRICE"/>
		<property name="curprice" type="integer" column="CURPRICE"/>
		<property name="dupprice" type="string" column="dupprice"/>
		<property name="needpoint" type="integer" column="NEEDPOINT"/>
		<property name="countdown" type="integer" column="COUNTDOWN"/>
		<property name="ordernum" type="integer" column="ORDERNUM"/>
		<property name="begintime" type="timestamp" column="BEGINTIME"/>
		<property name="endtime" type="timestamp" column="ENDTIME"/>
		<property name="logo" type="string" column="LOGO"/>
		<property name="source" type="string" column="SOURCE"/>
		<property name="remark" type="string" column="REMARK"/>
		<property name="nickname" type="string" column="NICKNAME"/>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="lasttime" type="timestamp" column="LASTTIME"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="saletype" type="string" column="SALETYPE"/>
		<property name="cardpass" type="string" column="CARDPASS"/>
		<property name="citycode" type="string" column="CITYCODE"/>
		<property name="description" type="string" column="DESCRIPTION"/>
		<property name="pubnumber" type="integer" column="PUBNUMBER" />
		<property name="goodsid" type="long" column="GOODSID" />
		<property name="unitMinute" type="integer" column="UNITMINUTE" />
		<property name="pubperiod" type="integer" column="PUBPERIOD" />
	</class>
</hibernate-mapping>
