<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.RepeatingPayorder" table="repeating_payorder" lazy="false">
		<id name="recordId" type="string" column="RECORDID">
			<generator class="assigned"/>
		</id>
		<property name="tradeNo" type="string" column="TRADE_NO" />
		<property name="payseqNo" type="string" column="PAYSEQNO" />
		<property name="payMethod" type="string" column="PAYMETHOD" />
		<property name="notifyTime" type="timestamp" column="NOTIFY_TIME" />
		<property name="status" type="string" column="status" />
		<property name="amount" type="integer" column="amount" />
		<property name="successPayMethod" type="string" column="SUCCESS_PAYMETHOD" />
		<property name="confirmUser" type="string" column="CONFIRM_USER" />
		<property name="confirmTime" type="timestamp" column="CONFIRM_TIME" />
	</class>
</hibernate-mapping>
