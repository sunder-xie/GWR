<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.SdRecord" table="SDRECORD" lazy="false">
		<id name="tradeNo" type="string" column="TRADENO" >
			<generator class="assigned" />
		</id>
		<property name="spcounterid" type="long" column="SPCOUNTERID"/>
		<property name="quantity" type="integer" column="QUANTITY"/>
		<property name="validtime" type="timestamp" column="VALIDTIME"/>
		<property name="cpcounters" type="string" column="CPCOUNTERS"/>
	</class>
</hibernate-mapping>
