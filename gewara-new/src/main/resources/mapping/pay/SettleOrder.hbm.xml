<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.SettleOrder" table="SETTLE_ORDER" lazy="false">
		<id name="orderid" type="long" column="RECORDID">
			<generator class="assigned" />
		</id>
		<property name="credentialsId" type="long" column="CREDENTIALSID"/>
		<property name="paytime" type="timestamp" column="PAYTIME"/>
	</class>
</hibernate-mapping>
