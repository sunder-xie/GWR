<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.Spcounter" table="SPCOUNTER" lazy="false" dynamic-update="true">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<version name="version" column="DATA_VERSION" type="integer"/>
		<property name="ctlmember" type="string" column="CTLMEMBER"/>
		<property name="ctltype" type="string" column="CTLTYPE" />
		<property name="limitmaxnum" type="integer" column="LIMITMAXNUM"/>
		<property name="allowaddnum" type="integer" column="ALLOWADDNUM"/>
		<property name="basenum" type="integer" column="BASENUM"/>
		<property name="periodtime" type="timestamp" column="PERIODTIME"/>
		<property name="periodMinute" type="integer" column="PERIODMINUTE"/>
		<property name="sellquantity" type="integer" column="SELLQUANTITY"/>
		<property name="sellordernum" type="integer" column="SELLORDERNUM"/>
		<property name="allquantity" type="integer" column="ALLQUANTITY"/>
		<property name="allordernum" type="integer" column="ALLORDERNUM"/>
	</class>
</hibernate-mapping>
