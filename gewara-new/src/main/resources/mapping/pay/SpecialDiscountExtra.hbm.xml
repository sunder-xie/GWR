<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.pay.SpecialDiscountExtra" table="SPECIALDISCOUNT_EXTRA" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="assigned" />
		</id>
		<property name="applycity" type="string" column="APPLYCITY" />
		<property name="applydept" type="string" column="APPLYDEPT" />
		<property name="applytype" type="string" column="APPLYTYPE" />
		<property name="allowancetype" type="string" column="ALLOWANCETYPE" />
		
		<property name="partnername" type="string" column="PARTNERNAME" />
		<property name="partnerid" type="long" column="PARTNERID" />
		<property name="orderAllowance" type="double" column="ORDERALLOWANCE" />
		<property name="unitAllowance" type="double" column="UNITALLOWANCE" />
		<property name="maxAllowance" type="double" column="MAXALLOWANCE" />
		
		<property name="partnername1" type="string" column="PARTNERNAME1" />
		<property name="partnerid1" type="long" column="PARTNERID1" />
		<property name="orderAllowance1" type="double" column="ORDERALLOWANCE1" />
		<property name="unitAllowance1" type="double" column="UNITALLOWANCE1" />
		<property name="maxAllowance1" type="double" column="MAXALLOWANCE1" />
		
		<property name="partnername2" type="string" column="PARTNERNAME2" />
		<property name="partnerid2" type="long" column="PARTNERID2" />
		<property name="orderAllowance2" type="double" column="ORDERALLOWANCE2" />
		<property name="unitAllowance2" type="double" column="UNITALLOWANCE2" />
		<property name="maxAllowance2" type="double" column="MAXALLOWANCE2" />
		
	</class>
</hibernate-mapping>
