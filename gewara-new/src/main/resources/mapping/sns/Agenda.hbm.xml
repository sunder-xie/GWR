<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.user.Agenda" table="AGENDA" lazy="false">
	 	<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="membername" type="string" column="MEMBERNAME"/>
		<property name="title" type="string" column="TITLE"/>
		<property name="content" type="string" column="CONTENT"/>
		<property name="startdate" type="date" column="START_DATE"/>
		<property name="starttime" type="string" column="START_TIME"/>
		<property name="tag" type="string" column="TAG"/>
		<property name="relatedid" type="long" column="RELATEDID"/>
		<property name="category" type="string" column="CATEGORY"/>
		<property name="categoryid" type="long" column="CATEGORYID"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="actionid" type="long" column="ACTIONID"/>
		<property name="action" type="string" column="ACTION"/>
		<property name="address" type="string" column="ADDRESS"/>
		<property name="enddate" type="date" column="END_DATE"/>
		<property name="endtime" type="string" column="END_TIME"/>
		<property name="otherinfo" type="string" column="OTHERINFO"/>
	 </class>
</hibernate-mapping>
