<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.user.HiddenMember" table="HIDDENMEMBER"  lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="realname" type="string" column="REALNAME"/>
		<property name="mobile" type="string" column="MOBILE"/>
		<property name="email" type="string" column="EMAIL"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		<property name="inviteid" type="long" column="INVITEID" />
	</class>
</hibernate-mapping>
