<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.user.InvoiceRelate" table="INVOICE_RELATE" lazy="false">
		<id name="tradeNo" type="string" column="TRADE_NO">
			<generator class="assigned" />
		</id>
		<property name="orderid" type="long" column="ORDERID" />
		<property name="invoiceid" type="long" column="INVOICEID"/>
		<property name="memberid" type="long" column="MEMBERID"/>
	 </class>
</hibernate-mapping>
