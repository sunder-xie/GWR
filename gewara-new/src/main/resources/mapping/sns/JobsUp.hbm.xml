<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.user.JobsUp" table="JOBSUP"  lazy="false">
		<id name="id" type="long" column="recordid" >
			<generator class="sequence" />
		</id>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="jobsid" type="long" column="JOBSID"/>
		<property name="position" type="string" column="POSITION"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
	</class>
</hibernate-mapping>
