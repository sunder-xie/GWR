<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.user.MemberInfoMore" table="MEMBERINFOMORE"  lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="memberid" type="long" column="MEMBERID" />
		<property name="tag" type="string" column="TAG"/>
		<property name="schooladdress" type="string" column="SCHOOLADDRESS"/>
		<property name="name" type="string" column="NAME"/>
		<property name="schooltype" type="string" column="SCHOOLTYPE"/>
		<property name="educomeinyear" type="string" column="EDUCOMEINYEAR"/>
		<property name="jobprovince" type="string" column="JOBPROVINCE"/>
		<property name="jobcity" type="string" column="JOBCITY"/>
		<property name="jobdep" type="string" column="JOBDEP"/>
		<property name="jobcompanyemail" type="string" column="JOBCOMPANYEMAIL"/>
	</class>
</hibernate-mapping>
