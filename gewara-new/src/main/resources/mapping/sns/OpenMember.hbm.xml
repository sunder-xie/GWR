<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.user.OpenMember" table="OPENMEMBER"  lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="loginname" type="string" column="LOGINNAME"/>
		<property name="source" type="string" column="SOURCE"/>
		<property name="category" type="string" column="CATEGORY"/>
		<property name="nickname" type="string" column="NICKNAME"/>
		<property name="otherinfo" type="string" column="OTHERINFO"/>
		<property name="relateid" type="long" column="RELATEID"/>
		<property name="validtime" type="timestamp" column="VALIDTIME"/>
	</class>
</hibernate-mapping>
