<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.user.TempMember" table="TEMP_MEMBER" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence"/>
		</id>
		<property name="mobile" type="string" column="MOBILE"/>
		<property name="tmppwd" type="string" column="TMPPWD"/>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="flag" type="string" column="FLAG"/>
		<property name="otherinfo" type="string" column="OTHERINFO"/>
		<property name="ip" type="string" column="IP"/>
		<property name="membertype" type="string" column="MEMBERTYPE"/>
		<property name="status" type="string" column="STATUS"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
	</class>
</hibernate-mapping>
