<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.sport.Guarantee" table="GUARANTEE" dynamic-update="true" lazy="false">
		<cache usage="read-write" region="halfDay"/>
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="name" type="string" column="NAME" />
		<property name="price" type="integer" column="PRICE" />
		<property name="costprice" type="integer" column="COSTPRICE" />
		<property name="otherinfo" type="string" column="OTHERINFO" />
		<property name="status" type="string" column="STATUS" />
		<property name="citycode" type="string" column="CITYCODE" />
		<property name="remark" type="string" column="REMARK" />
		<property name="description" type="string" column="DESCRIPTION" />
		<property name="createuser" type="long" column="CREATEUSER" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="updatetime" type="timestamp" column="UPDATETIME" />
		<property name="ordermsg" type="string" column="ORDERMSG" />
	</class>
</hibernate-mapping>
