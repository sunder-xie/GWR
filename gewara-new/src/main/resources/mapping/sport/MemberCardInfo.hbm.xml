<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.sport.MemberCardInfo" table="MEMBER_CARD_INFO"  lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="typeid" type="long" column="TYPEID"/>
		<property name="tradeno" type="string" column="TRADENO"/>
		<property name="cardTypeUkey" type="string" column="CARDTYPEUKEY"/>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="memberCardCode" type="string" column="MEMBERCARDCODE"/>
		<property name="name" type="string" column="NAME"/>
		<property name="sex" type="string" column="SEX"/>
		<property name="mobile" type="string" column="MOBILE"/>
		<property name="overMoney" type="integer" column="OVERMONEY"/>
		<property name="validtime" type="timestamp" column="VALIDTIME"/>
		<property name="cardStatus" type="string" column="CARDSTATUS"/>
		<property name="fitItem" type="string" column="FITITEM"/>
		<property name="belongVenue" type="string" column="BELONGVENUE"/>
		<property name="typetitle" type="string" column="TYPETITLE"/>
		<property name="bindStatus" type="string" column="BINDSTATUS"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
	</class>
</hibernate-mapping>
