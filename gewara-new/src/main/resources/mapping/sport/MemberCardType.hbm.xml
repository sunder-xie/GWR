<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.sport.MemberCardType" table="MEMBER_CARD_TYPE"  lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="cardTypeUkey" type="string" column="CARDTYPEUKEY"/>
		<property name="cardTypeCode" type="string" column="CARDTYPECODE"/>
		<property name="cardType" type="string" column="CARDTYPE"/>
		<property name="money" type="integer" column="MONEY"/>
		<property name="overNum" type="integer" column="OVERNUM"/>
		<property name="reserve" type="integer" column="RESERVE"/>
		<property name="description" type="string" column="DESCRIPTION"/>
		<property name="validTime" type="integer" column="VALIDTIME"/>
		<property name="price" type="integer" column="PRICE"/>
		<property name="discount" type="integer" column="DISCOUNT"/>
		<property name="fitItem" type="string" column="FITITEM"/>
		<property name="belongVenue" type="string" column="BELONGVENUE"/>
		<property name="businessId" type="long" column="BUSINESSID"/>
		
		<property name="gewaprice" type="integer" column="GEWAPRICE"/>
		<property name="sales" type="integer" column="SALES"/>
		<property name="costRate" type="integer" column="COSTRATE"/>
		<property name="mingain" type="integer" column="MINGAIN"/>
		
		<property name="status" type="string" column="STATUS"/>
		<property name="opentime" type="timestamp" column="OPENTIME"/>
		<property name="closetime" type="timestamp" column="CLOSETIME"/>
		<property name="spflag" type="string" column="SPFLAG"/>
		<property name="minpoint" type="integer" column="MINPOINT"/>
		<property name="maxpoint" type="integer" column="MAXPOINT"/>
		<property name="elecard" type="string" column="ELECARD"/>
		<property name="remark" type="string" column="REMARK"/>
		<property name="otherinfo" type="string" column="OTHERINFO"/>
		
		<property name="notifymsg" type="string" column="NOTIFYMSG"/>
		
		<property name="addtime" type="timestamp" column="ADDTIME"/>
		
	</class>
</hibernate-mapping>
