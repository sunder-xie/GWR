<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.sport.OpenTimeSale" table="OPEN_TIMESALE" dynamic-update="true" lazy="false">
		<cache usage="read-write" region="sportTicket"/>
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="sportid" type="long" column="SPORTID" />
		<property name="itemid" type="long" column="ITEMID" />
		<property name="fieldid" type="long" column="FIELDID" />
		<property name="ottid" type="long" column="OTTID" />
		<property name="bindInd" type="string" column="BINDIND" />
		<property name="lowerprice" type="integer" column="LOWERPRICE" />
		<property name="curprice" type="integer" column="CURPRICE" />
		<property name="dupprice" type="integer" column="DUPPRICE" />
		<property name="auctionprice" type="integer" column="AUCTIONPRICE" />
		<property name="otiids" type="string" column="OTIIDS" />
		<property name="starttime" type="string" column="STARTTIME" />
		<property name="endtime" type="string" column="ENDTIME" />
		<property name="version" type="integer" column="VERSION" />
		<property name="nickname" type="string" column="NICKNAME" />
		<property name="memberid" type="long" column="MEMBERID" />
		<property name="playdate" type="date" column="PLAYDATE" />
		<property name="opentime" type="timestamp" column="OPENTIME" />
		<property name="closetime" type="timestamp" column="CLOSETIME" />
		<property name="joinnum" type="integer" column="JOINNUM" />
		<property name="validtime" type="timestamp" column="VALIDTIME" />
		<property name="paidvalidtime" type="timestamp" column="PAIDVALIDTIME" />
		<property name="jointime" type="timestamp" column="JOINTIME" />
		<property name="status" type="string" column="STATUS" />
		<property name="citycode" type="string" column="CITYCODE" />
		<property name="addtime" type="timestamp" column="ADDTIME" />
		<property name="lockStatus" type="string" column="LOCKSTATUS" />
		<property name="guaranteeid" type="long" column="GUARANTEEID" />
		<property name="otherinfo" type="string" column="OTHERINFO" />
		<property name="orderid" type="long" column="ORDERID" />
		<property name="mobile" type="string" column="MOBILE" />
		<property name="message" type="string" column="MESSAGE" />
	</class>
</hibernate-mapping>
