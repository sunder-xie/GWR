<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.sport.OpenTimeSaleMember" table="OPEN_TIMESALE_MEMBER" lazy="false">
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="otsid" type="long" column="OTSID"/>
		<property name="memberid" type="long" column="MEMBERID"/>
		<property name="nickname" type="string" column="NICKNAME"/>
		<property name="price" type="integer" column="PRICE"/>
		<property name="dupprice" type="integer" column="DUPPRICE"/>
		<property name="addtime" type="timestamp" column="ADDTIME"/>
	</class>
</hibernate-mapping>
