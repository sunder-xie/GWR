<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.sport.Sport2Item" table="SPORT_SPORTITEM" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="sportid" type="long" column="SPORTID"/>
		<property name="itemid" type="long" column="SPORTITEMID"/>
		<property name="minprice" type="integer" column="MINPRICE"/>
		<property name="maxprice" type="integer" column="MAXPRICE"/>
		<property name="avgprice" type="integer" column="AVGPRICE"/>
		<property name="takemethod" type="string" column="TAKEMETHOD"/>
		<property name="notifymsg1" type="string" column="NOTIFYMSG1"/>
		<property name="notifymsg2" type="string" column="NOTIFYMSG2"/>
		<property name="notifymsg3" type="string" column="NOTIFYMSG3"/>
		<property name="overmsg" type="string" column="OVERMSG"/>
		<property name="booking" type="string" column="BOOKING"/>
		<property name="sortnum" type="integer" column="SORTNUM"/>
		
		<property name="limitminutes" type="integer" column="limitminutes"/>
		<property name="diaryid" type="long" column="diaryid"/>
		<property name="tickettype" type="string" column="tickettype"/>
		<property name="exitsreturn" type="string" column="exitsreturn"/>
		<property name="returnminutes" type="integer" column="returnminutes"/>
		<property name="returnmoneytype" type="string" column="returnmoneytype"/>
		<property name="returnmoney" type="double" column="returnmoney"/>
		<property name="exitschange" type="string" column="exitschange"/>
		<property name="changeminutes" type="integer" column="changeminutes"/>
		<property name="changemoneytype" type="string" column="changemoneytype"/>
		<property name="changemoney" type="double" column="changemoney"/>
		<property name="returnsmallmoney" type="integer" column="return_money_min"/>
		<property name="changesmallmoney" type="integer" column="change_money_min"/>
		<property name="otherinfo" type="string" column="OTHERINFO" />
		<property name="description" type="string" column="DESCRIPTION" />
		<property name="sporttype" type="string" column="SPORTTYPE" />
		<property name="opentype" type="string" column="OPENTYPE" />
		<property name="createtype" type="string" column="CREATETYPE" />
		<property name="cycle" type="integer" column="CYCLE" />
		<property name="opentime" type="string" column="OPENTIME" />
		<property name="closetime" type="string" column="CLOSETIME" />
		<property name="prompting" type="string" column="PROMPTING" />
	</class>
</hibernate-mapping>
