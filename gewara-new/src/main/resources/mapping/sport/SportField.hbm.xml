<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.sport.SportField" table="SPORT_FIELD" lazy="false">
		<id name="id" type="long" column="RECORDID" >
			<generator class="sequence" />
		</id>
		<property name="sportid" type="long" column="SPORTID"/>
		<property name="itemid" type="long" column="ITEMID"/>
		<property name="name" type="string" column="NAME"/>
		<property name="ordernum" type="integer" column="ORDERNUM"/>
		<property name="description" type="string" column="DESCRIPTION"/>
		<property name="remoteid" type="long" column="REMOTEID"/>
		<property name="status" type="string" column="STATUS"/>
	</class>
</hibernate-mapping>
