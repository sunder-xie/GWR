<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.ticket.OpenPlayItem" table="OPEN_PLAYITEM" dynamic-update="true" lazy="false">
		<cache usage="read-write" region="cinemaTicket"/>
		<id name="id" type="long" column="RECORDID">
			<generator class="sequence" />
		</id>
		<property name="mpid" type="long" column="MPID"/>
		<property name="cinemaid" type="long" column="CINEMAID"/>
		<property name="movieid" type="long" column="MOVIEID"/>
		<property name="roomid" type="long" column="ROOMID"/>
		<property name="cinemaname" type="string" column="CINEMANAME"/>
		<property name="moviename" type="string" column="MOVIENAME"/>
		<property name="roomname" type="string" column="ROOMNAME"/>
		<property name="playtime" type="timestamp" column="PLAYTIME" />
		<property name="edition" type="string" column="EDITION" />
		<property name="language" type="string" column="LANGUAGE" />
		<property name="opentime" type="timestamp" column="OPENTIME" />
		<property name="closetime" type="timestamp" column="CLOSETIME" />
		<property name="price" type="integer" column="PRICE" />
		<property name="costprice" type="integer" column="COSTPRICE" />
		<property name="gewaprice" type="integer" column="GEWAPRICE" />
		<property name="lowest" type="integer" column="LOWEST" />
		<property name="status" type="string" column="STATUS"/>
		<property name="partner" type="string" column="PARTNER"/>
		<property name="remark" type="string" column="REMARK"/>
		<property name="opentype" type="string" column="OPENTYPE" />
		<property name="elecard" type="string" column="ELECARD" />
		<property name="seqNo" type="string" column="SEQNO"/>
		<property name="topicid" type="long" column="TOPICID"/>
		<property name="minpoint" type="integer" column="MINPOINT" />
		<property name="maxpoint" type="integer" column="MAXPOINT" />
		<property name="buylimit" type="string" column="BUYLIMIT"/>
		<property name="dayotime" type="string" column="DAYOTIME"/>
		<property name="dayctime" type="string" column="DAYCTIME"/>
		<property name="seatnum" type="integer" column="SEATNUM" />
		<property name="asellnum" type="integer" column="ASELLNUM" />
		<property name="gsellnum" type="integer" column="GSELLNUM" />
		<property name="csellnum" type="integer" column="CSELLNUM" />
		<property name="locknum" type="integer" column="LOCKNUM" />
		<property name="updatetime" type="timestamp" column="UPDATETIME" />
		<property name="citycode" type="string" column="CITYCODE"/>
		<property name="givepoint" type="integer" column="GIVEPOINT"/>
		<property name="otherinfo" type="string" column="OTHERINFO"/>
		<property name="spflag" type="string" column="SPFLAG"/>
		<property name="fee" type="integer" column="FEE" />
		<property name="expressid" type="string" column="EXPRESSID" />
	</class>
</hibernate-mapping>
