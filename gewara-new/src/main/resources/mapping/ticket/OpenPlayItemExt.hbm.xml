<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.ticket.OpenPlayItemExt" table="OPEN_PLAYITEM_EXT" dynamic-update="true" lazy="false">
		<id name="mpid" type="long" column="MPID">
			<generator class="assigned" />
		</id>
		<property name="actualprice" type="integer" column="ACTUALPRICE"/>
		<property name="totalcost" type="integer" column="TOTALCOST"/>
		<property name="seatnum" type="integer" column="SEATNUM"/>
		<property name="createuser" type="long" column="CREATEUSER" />
		<property name="createtime" type="timestamp" column="CREATETIME" />
		<property name="opentime" type="timestamp" column="OPENTIME" />
		<property name="openuser" type="long" column="OPENUSER" />
		<property name="delayMin" type="integer" column="DELAYMIN" />
		<property name="remark" type="string" column="REMARK" />
		<property name="settle" type="string" column="SETTLE" />
		<property name="imprest" type="string" column="IMPREST" />
	</class>
</hibernate-mapping>
