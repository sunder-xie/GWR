<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.ticket.OrderOrigin" table="ORDERORIGIN" lazy="false">
		<id name="tradeNo" type="string" column="TRADENO">
			<generator class="assigned" />
		</id>
		<property name="origin" type="string" column="ORIGIN"/>
		<property name="appSource" type="string" column="APPSOURCE"/>
		<property name="partnerid" type="long" column="PARTNERID"/>
		<property name="addtime" type="timestamp" column="ADDTIME" />
	</class>
</hibernate-mapping>