<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gewara.model.api.OrderResult" table="ORDERRESULT" lazy="false">
		<id name="tradeno" type="string" column="TRADENO">
			<generator class="assigned" />
		</id>
		<property name="taketime" type="timestamp" column="TAKETIME"/>
		<property name="caption" type="string" column="CAPTION"/>
		<property name="istake" type="string" column="ISTAKE"/>
		<property name="conflict" type="string" column="CONFLICT"/>
		<property name="result" type="string" column="RESULT"/>
		<property name="ticketnum" type="integer" column="TICKETNUM"/>
		<property name="updatetime" type="timestamp" column="UPDATETIME"/>
		<property name="ordertype" type="string" column="ORDERTYPE"/>
		<property name="placeid" type="long" column="PLACEID"/>
	</class>
</hibernate-mapping>