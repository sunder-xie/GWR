<div id="cityForm">
	省份：$!{city.province.provincename}<br/>
	代码：<input type="text" name="citycode" value="$city.citycode" readonly="readonly"><br/>
	名称：<input type="text" name="cityname" value="$city.cityname" ><br/>
	拼音：<input type="text" name="pinyin" value="$!pinyinMap.get($city.citycode)"><br/>
	简拼：<input type="text" name="py" value="$!pyMap.get($city.citycode)"><br/>
	<input type="button" value="保存" onclick="saveGewaCity()">
</div>