<div class="citySearch clear">
	<h2 class="ffst">输入中文/拼音</h2>
	<ul>
		#if($VmUtils.isEmptyList($gewaCityList))
			<li>未查询到相关城市！</li>
		#else
			#foreach($city in $gewaCityList)
			<li><a href="${basePath}admin/changeCity.xhtml?citycode=$city.citycode#if($targetUrl)&path=$!targetUrl#end"><em class="right c999 mr5">$!{city.pinyin}($!city.py.toUpperCase())</em>$!city.cityname</a></li>
			#end
		#end
	</ul>
</div>
