<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<title>数据统计</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<link rel="stylesheet" type="text/css" href="${staticPath}css/gewara.css?n=admin"/>
	<script type="text/javascript" src="${staticPath}scripts/mootools.js"></script>
	<script type="text/javascript" src="${staticPath}scripts/gewara-util.js?v=$VmUtils.jsVersion"></script>
	<script type="text/javascript" src="${staticPath}dpicker/WdatePicker.js"></script>
	<style type="text/css">
		.match{color:#fff;font-weight: bolder;background:#fff;padding:3px;}
		.shrink{height:22px;overflow:hidden;}
		td, th{
			text-align: center;
			height: 23px;
		}
	</style>
</head>
<body>
#set($isShare=true)
#parse('admin/datacount/menuMember.vm')
<div style="margin-left: 140px;width: 800px;background-color: #fff;height: auto;">
		<form action="${basePath}admin/datacount/member/shareCount.xhtml" id="form">
		<div style="margin-left: 15px;margin-bottom: 5px;">
			开始时间：<input name="starttime" id="starttime" readonly="readonly" onclick="WdatePicker({dateFmt:'yyyy-MM-dd 00:00:00'})" value="$!starttime" type="text" style="width: 160px"/>
			结束时间：<input name="endtime" id="endtime" readonly="readonly" onclick="WdatePicker({dateFmt:'yyyy-MM-dd 23:59:59'})" value="$!endtime" type="text" style="width: 160px"/>
			<input type="submit" value="查询" />
		</div>
		</form>
<div>
#end 
#set($keyList=["日期","共计","购票","活动","帖子","影评","剧评","生活","哇啦"])
<table cellpadding="0" cellspacing="0" width="100%" border="1" style="border-collapse: collapse">
	<tr>
		#foreach($key in $keyList)
		<th>$key</th>
		#end
	</tr>
	#foreach($qryMap in $qryMapList)
	<tr>
		#foreach($key in $keyList)
		<td>$!qryMap.get($key)</td>
		#end
	</tr>
	#end
	<tr>
	<th>合计</th>
	#foreach($key in $keyList)
		#if($foreach.count gt 1)
		<th>$VmUtils.getTotal($qryMapList, $key)</th>
		#end
	#end
	</tr>
</table>
#if(!$params.isxls)
</div>
</div>
<div>
<a href="http://open.t.sina.com.cn/apps/2536251945" target="_blank">新浪微博</a>
<br/>app@gewara.com gewara3026
</div>
	<script type="text/javascript">
	$$("tr").addEvents({
		'mouseover':function(){
			this.addClass("mouseBg");
		},
		'mouseout':function(){
			this.removeClass("mouseBg");
		}
	});
</script>
</body>
</html>
#end