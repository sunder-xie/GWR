.text{height:20px; padding:5px; font:bold 16px/20px verdana; color:#333; border-radius:5px; width:183px;}
.photoNum{font-size:18px; color:#333; font-weight:bold; font-family:verdana; vertical-align:-2px;}

.btn{font:14px/30px '微软雅黑',verdana,'宋体'; border-radius:3px; cursor:pointer; height:30px; line-height:30px; padding:0 20px; display:inline-block;}
.redBtn{background:#eb6120; color:#fff; border:1px solid #c53; border-left:#ff693e; border-top:#ff693e;}
.redBtn:hover{text-decoration:none; color:#fff; background:#f87000;}
.whiteBtn{background:#f2f2f2; color:#666; border:1px solid #e3e3e3; border-left:#dedede; border-top:#dedede;}
.whiteBtn:hover{text-decoration:none; color:#666; background:#f7f7f7;}
.disBtn,.disBtn:hover{background:#f5f5f5; color:#d5d5d5; border:1px solid #f5f5f5; border-left:#dedede; border-top:#dedede;}

.ui_media select{height:32px; line-height:32px; width:195px; background:#fff; border:1px solid #bbb; border-radius:5px; padding:6px 5px; display:inline-block; vertical-align:middle;}
.successNotice img{display:inline-block; width:40px; height:40px; background:url(${staticPath}css/global/ico_types.gif) 0 0 no-repeat;}
.successNotice h2{font:bold 16px/40px '微软雅黑',verdana,'宋体'; color:#333;}

/*步骤*/
.payStep02,.payStep01,.payStep03{overflow: hidden;margin:16px 0;}
.step1,.step2,.step3{text-indent:-8px; background:url(${staticPath}css/global/steps.gif) no-repeat;}
