.mod_tLine .mod_hd h2{font-family:'微软雅黑','宋体'; font-size:16px; font-weight:bold;}

.modifyBox{border:1px solid #eee; line-height:30px; margin-top:10px; padding:10px; color:#999;}

.safetyList ul{border:1px solid #eee; border-bottom:none;}
