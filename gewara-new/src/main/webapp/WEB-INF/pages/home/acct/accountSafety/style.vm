.payStep01 .step2,.payStep03 .step1{background-position:right -68px;}
.payStep01 .step3,.payStep02 .step3,.payStep03 .step3{background-image:none;}

.modifyBox .ui_media,.ui_text{overflow:hidden; *overflow:visible; zoom:1;}
.modifyBox .ui_media .ui_pic img,.ui_media .ui_picR img{display:block;}
.modifyBox .ui_media .ui_pic{float:left; width:200px; text-align:right;}
.modifyBox .ui_media .ui_picR{margin-left:10px; float:right;}
.modifyBox .ui_media .ui_media{margin-top:10px; color:#666;}

.modifyBox li{padding:30px 0;}
.modifyBox li.line{border-bottom:1px solid #eee;}

