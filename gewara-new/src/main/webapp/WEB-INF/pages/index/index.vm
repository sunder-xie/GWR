								#set($relate=$relatedHelper.getR1("gcMovieDiary",$diary.id))
								<dl>#set($relate1=$relatedHelper.getR1("categoryMap",$relate.categoryid))
									<dt><a href="${basePath}blog/t$!relate.id" target="_blank" title="$!relate.subject">$!VmUtils.escabbr($!relate.subject,30)</a>
										<span class="more">
											<a href="${basePath}home/sns/othersPersonIndex.xhtml?memberid=$!relate.memberid" target="_blank" title="$!cacheMemberMap.get($relate.memberid).nickname"><img width="16" height="16" src="${staticPath}css/images/blank.gif" data-href="$VmUtils.randomPic('cw16h16',$cacheMemberMap.get($relate.memberid).headpicUrl)"/></a>
											<a href="${basePath}home/sns/othersPersonIndex.xhtml?memberid=$!relate.memberid" target="_blank" class="ml5 mr5" title="$!cacheMemberMap.get($relate.memberid).nickname">$!VmUtils.escabbr($cacheMemberMap.get($relate.memberid).nickname,8)</a>
											#if($relate1)评:<a href="${basePath}movie/$relate.categoryid" target="_blank" class="ml5" title="$!relate1.name">$!VmUtils.escabbr($!relate1.name,8)</a>#end
										</span>
									</dt>
									<dd>$!VmUtils.getHtmlText($!relate.summary, 110)..<a class="ffst ml5" href="${basePath}blog/t$!relate.id" target="_blank">全文</a></dd> 
								</dl>
								#if($foreach.index le 0)
								<div class="mt10 dashed"></div>
								#end
								#end
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="ui_indexBoxRight">
				<div class="activityBox activityMovie">
					<div class="title">
						<h2>活动进行中..</h2>
						<span class="ac_List"><a class="mr5" title="查看全部活动" href="${basePath}activity/activityList.xhtml?tag=movie" target="_blank">全部</a><em id="movieSlideCount">1</em>/$!VmUtils.size($gcMovieAct)</span>
						<span class="ac_prew" title="上一个" id="movie_move_top"></span>
						<span class="ac_next" title="下一个" id="movie_move_bottom"></span>
					</div>
					<div class="content">
						<span class="leftZone"></span>
						<div class="contentInner">
							#set($height = $!VmUtils.size($gcMovieAct) * 240)
							<ul id="movie_slide_box" style="height:${height}px;">
								#foreach($activity in $gcMovieAct)
								#set($relate = $relatedHelper.getR1("gcMovieAct",$activity.id))
								<li>
									<a title="$activity.title" href="${basePath}activity/$!relate.id" target="_blank"><img width="235" height="180" alt="$activity.title" #if($foreach.count eq 1) src="$VmUtils.randomPic('',$!activity.logo)" #else src="${staticPath}css/images/blank.gif" data-href="$VmUtils.randomPic('',$!activity.logo)" #end /></a>
									<p class="mt5"><a href="${basePath}activity/$!relate.id" target="_blank" title="$!activity.title">$!VmUtils.escabbr($activity.title, 36)</a></p>
									<p>截止日期：$DateUtil.format($!relate.enddate, 'MM月dd日')<span class="c999 ml15">$!relate.clickedtimes人关注</span></p>
								</li>
								#end
							</ul>
						</div>
						<span class="rightZone"></span>
					</div>
				</div>
				<div class="ui_lbAdbox">
					#foreach($member in $gcMovieMem)
					<h3>$!member.title</h3>
					<a class="mt5 ui_slide" title="$!member.summary" href="$!member.link" target="_blank"><img src="${staticPath}css/images/blank.gif" data-href="$VmUtils.randomPic('',$!member.logo)" width="260" height="90"/></a>
					#end
				</div>
			</div>
		</div>
	</div>
</div>
