						</div>
						<div class="even_move" id="menu_box_sport">
							<span class="even_left"><b class="orientation"></b></span>
							<span class="even_right"><b class="orientation"></b></span>
							<div class="even_center">
								<ul id="menu_box_sport_inner">
									#foreach($sport in $newgcSportList)
									#set($relate=$relatedHelper.getR1("newgcSportList", $sport.id))
									<li>
										<div class="even_img" title="$!relate.itemname">
											<a href="${basePath}sport/sportList.xhtml?servicetype=$!relate.id" target="_blank"><img width="120" height="160" alt="$!relate.itemname" src="${staticPath}css/images/blank.gif" data-href="$VmUtils.randomPic('cw120h160', $sport.limg)"/></a>
