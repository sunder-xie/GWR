									#end
								#end
								
								#foreach($news in $gcMovieNews)
								<li><span class="ffst">[资讯]</span><a title="$!news.title" href="${basePath}news/$!news.relatedid" target="_blank">$!VmUtils.escabbr($!news.title, 60)</a></li>
