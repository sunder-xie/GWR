	new PanelGroup(['tapsInMovie', 'tapsInDrama','tapsInSport'], 'tapsInMovie', "select", "none", "mouseover");
	new sigleLeep('menu_box_movie','menu_box_movie_inner',{isNode:'li','disable':'disable','size':132,'length':5,'screen':132});
	new sigleLeep('menu_box_drama','menu_box_drama_inner',{isNode:'li','disable':'disable','size':132,'length':5,'screen':132});
