<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
<body>
	#if(!$submitUrl)#set($submitUrl=$payUrl)#end
	<form action="$submitUrl" id="submitForm" name="submitForm" method="${method}">
		#foreach($key in $paramsNames)
		<textarea name="$key" style="display: none;">$!submitParams.get($key)</textarea>
		#end
		#if($pause)<input type="submit" value="提交" />#end
	</form>
	#if(!$pause)
	<script language="javascript">
		document.getElementById("submitForm").submit();
	</script>
	#end
</body>
</html>
