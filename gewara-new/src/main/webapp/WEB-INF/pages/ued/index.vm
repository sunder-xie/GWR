<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<title>前端组 - 格瓦拉生活网</title>
	<link rel="stylesheet" type="text/css" href="${staticPath}css/gewara.css?n=layout,ued&v=$VmUtils.jsVersion"/>
	<script type="text/javascript" src="${staticPath}scripts/mootools.js"></script>
	<script type="text/javascript" src="${basePath}scripts/gewara.js?n=gewara-util&v=$VmUtils.jsVersion"></script>
	<style type="text/css">
		/*带方向的提示*/
		.ui_tips_arrow_top,.ui_tips_arrow_right,.ui_tips_arrow_btm,.ui_tips_arrow_left{background:url(/css/global/ico_tips.gif?v=000001) no-repeat;}
		.ui_tips_success,.ui_tips_warning,.ui_tips_error{z-index:11;}
		.ui_tips .inner{position:relative;}
		.ui_tips_content{white-space:nowrap; height:24px; line-height:24px; line-height:26px\9; overflow:hidden; padding:0 10px;}
		.ui_tips_success .ui_tips_content{background:#efc; border:1px solid #bc3;}
		.ui_tips_warning .ui_tips_content{background:#ffd; border:1px solid #ffe3bb;}
		.ui_tips_error .ui_tips_content{background:#fee; border:1px solid #fcc;}
		.ui_tips_arrow_top,.ui_tips_arrow_right,.ui_tips_arrow_btm,.ui_tips_arrow_left{position:absolute; display:block; overflow:hidden; line-height:0; font-size:0;}
		.ui_tips_arrow_top,.ui_tips_arrow_btm{width:13px; height:7px; left:10px;}
		.ui_tips_arrow_left,.ui_tips_arrow_right{width:7px; height:13px; top:6px;}
		
		.ui_tips_arrow_top{top:-6px;}
		.ui_tips_arrow_right{right:-6px;}
		.ui_tips_arrow_btm{bottom:-6px;}
		.ui_tips_arrow_left{left:-6px;}
		
		.ui_tips_success .ui_tips_arrow_top{background-position:0 0;}
		.ui_tips_success .ui_tips_arrow_right{background-position:-6px 0;}
		.ui_tips_success .ui_tips_arrow_btm{background-position:0 -6px;}
		.ui_tips_success .ui_tips_arrow_left{background-position:0 0;}
		
		.ui_tips_warning .ui_tips_arrow_top{background-position:0 -13px;}
		.ui_tips_warning .ui_tips_arrow_right{background-position:-6px -13px;}
		.ui_tips_warning .ui_tips_arrow_btm{background-position:0 -19px;}
		.ui_tips_warning .ui_tips_arrow_left{background-position:0 -13px;}
		
		.ui_tips_error .ui_tips_arrow_top{background-position:0 -26px;}
		.ui_tips_error .ui_tips_arrow_right{background-position:-6px -26px;}
		.ui_tips_error .ui_tips_arrow_btm{background-position:0 -32px;}
		.ui_tips_error .ui_tips_arrow_left{background-position:0 -26px;}
	</style>
</head>
<body>
#set($cfgHeader='header')
#parse('include/navigation.vm')
<div class="ued_layout clear">
	<div class="ui_row_6">
		#parse('ued/items/sub_nav.vm')
	</div>
	<div class="ui_row_18">
		#*
		#parse('ued/cssRule.vm')
		*#
		#parse('ued/cssLibrary.vm')
	</div>
</div>
#parse('include/bottom.vm')
</body>
</html>