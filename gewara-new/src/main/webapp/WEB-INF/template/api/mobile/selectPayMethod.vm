<data>
	<payMethod>
		<tradeno>$!tradeno</tradeno>
		<discountAmount>$!discountAmount</discountAmount>
		<totalAmount>$!totalAmount</totalAmount>
		<due>$!due</due>
		<payurl><![CDATA[$!payUrl]]></payurl>
		<requestMethod>$!method</requestMethod>
		<payparams>
			#foreach($key in $paramsData.keySet())
			#if($VmUtils.isNotBlank($key))
			<param>
				<paramname><![CDATA[$!key]]></paramname>
				<paramvalue><![CDATA[$!paramsData.get($key)]]></paramvalue>
			</param>
			#end
			#end
		</payparams>
	</payMethod>
</data>
