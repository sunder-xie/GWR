<data>
	#if($lastUnpaidOrder)
	<lastUnpaidOrder>
		<orderId>$!lastUnpaidOrder.id</orderId>
		<tradeno>$!lastUnpaidOrder.tradeNo</tradeno>
		<payseqno>$!lastUnpaidOrder.payseqno</payseqno>
		<status>$!lastUnpaidOrder.statusText2</status>
		<isNew>$!lastUnpaidOrder.new</isNew>
		<ordertitle>$!lastUnpaidOrder.ordertitle</ordertitle>
		<description><![CDATA[$!lastUnpaidOrder.description2]]></description>
		<paidAmount>$!lastUnpaidOrder.alipaid</paidAmount>
		<quantity>$!lastUnpaidOrder.quantity</quantity>
		<paidtime>$!DateUtil.formatTimestamp($lastUnpaidOrder.paidtime)</paidtime>
		<orderType>$!lastUnpaidOrder.getOrdertype()</orderType>
	</lastUnpaidOrder>
	#end
	<orderList>
	#foreach($order in $orderList)
	<order>
		<orderId>$!order.id</orderId>
		<tradeno>$!order.tradeNo</tradeno>
		<payseqno>$!order.payseqno</payseqno>
		<status>$!order.statusText2</status>
		<ordertitle>$!order.ordertitle</ordertitle>
		<description><![CDATA[$!order.description2]]></description>
		<paidAmount>$!order.alipaid</paidAmount>
		<quantity>$!order.quantity</quantity>
		<paidtime>$!DateUtil.formatTimestamp($order.paidtime)</paidtime>
		<orderType>$!order.getOrdertype()</orderType>
	</order>
	#end
	</orderList>
</data>