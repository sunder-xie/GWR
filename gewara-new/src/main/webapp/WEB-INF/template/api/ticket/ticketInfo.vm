#if($xmlError)
<error>$errmsg</error>
#else
<data>
	<tradeNo>$tradeNo</tradeNo>
	<sportname>$sportname</sportname>
	#if($VmUtils.eq($!type,'sport'))
	<itemname>$itemname</itemname>
	<hour>$hour</hour>
	<totalfee>$sorder.totalfee</totalfee>
	<validtime>$validtime</validtime>
	<quantity>$sorder.quantity</quantity>
	#elseif($VmUtils.eq($!type,'drama'))
	<dramaname>$VmUtils.substring($dramaname, 0,9)</dramaname>
	<date>$date</date>
	<price>$price</price>
	<quantity>$quantity</quantity>	
	#elseif($VmUtils.eq($!type,'cinema'))
	<goodsname>$goodsname</goodsname>
	<unitprice>$unitprice</unitprice>
	<validtime>$validtime</validtime>
	<quantity>$quantity</quantity>
	<summary>$summary</summary>
	#end
</data>
#end
