<data>
	<joinList>
		#foreach($join in $joinList)
		#set($member=$memberMap.get($join.memberid))
		<join>
			<memberid>$!join.memberid</memberid>
			<nickname>$!member.nickname</nickname>
			<headpic>${mobilePath}$!member.headpicUrl</headpic>
			<joinnum>$!join.joinnum</joinnum>
			<addtime>$!DateUtil.formatTimestamp($join.addtime)</addtime>
			<sex>$!join.sex</sex>
			<birthday>$!member.birthday</birthday>
			#if($isAdmin)
			<contactway>$!join.contactway</contactway>
			#end
		</join>
		#end
	</joinList>
</data>
