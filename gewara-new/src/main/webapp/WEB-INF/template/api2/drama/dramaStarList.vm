<dramaStarList>
	#foreach($dramastar in $dramaStarList)
		<dramaStar>
			<gewaid>$!dramastar.id</gewaid>
			<name>$!dramastar.name</name>
			<birthday>$!dramastar.birthday</birthday>
			<state>$!dramastar.state</state>
			<bloodtype>$!dramastar.bloodtype</bloodtype>
			<constellation>$!dramastar.constellation</constellation>
			<height>$!dramastar.height</height>
			<hometown>$!dramastar.hometown</hometown>
			<updatetime>$!dramastar.updatetime</updatetime>
			<graduated>$!dramastar.graduated</graduated>
			<job>$!dramastar.job</job>
			<troupe>$!dramastar.troupe</troupe>
			<establishtime>$!dramastar.establishtime</establishtime>
			<startype>$!dramastar.startype</startype>
			<representative>$!dramastar.representative</representative>
			<representativeRelate>$!dramastar.representativeRelate</representativeRelate>
			<addtime>$!dramastar.addtime</addtime>
		</dramaStar>
	#end
</dramaStarList>
