<theatreRoomList>
	#foreach($room in $roomList)
		<theatreRoom>
			<gewaid>$!room.id</gewaid>
			<theatreid>$!room.theatreid</theatreid>
			<roomname>$!room.roomname</roomname>
			<linenum>$!room.linenum</linenum>
			<ranknum>$!room.ranknum</ranknum>
			<seatnum>$!room.seatnum</seatnum>
			<content>$!room.content</content>
			<updatetime>$!room.updatetime</updatetime>
		</theatreRoom>
	#end
</theatreRoomList>
