<theatreSeatPriceList>
	#foreach($price in $priceList)
		<theatreSeatPrice>
			<gewaid>$!price.id</gewaid>
			<odiid>$!odiMap.get($!price.id)</odiid>
			<seattype>$!price.seattype</seattype>
			<price>$!price.price</price>
			<costprice>$!price.costprice</costprice>
			<theatreprice>$!price.theatreprice</theatreprice>
			<status>$!price.status</status>
			<remark>$!price.remark</remark>
			<upcard>$!price.upcard</upcard>
			<dpid>$!price.dpid</dpid>
			<quantity>$!price.quantity</quantity>
			<sales>$!price.sales</sales>
			<updatetime>$!price.updatetime</updatetime>
		</theatreSeatPrice>
	#end
</theatreSeatPriceList>
