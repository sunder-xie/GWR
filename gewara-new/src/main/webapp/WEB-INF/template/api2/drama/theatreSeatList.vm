<openTheatreSeatList>
	#foreach($seat in $theareSeatList)
	<openTheatreSeat>
		<gewaid>$!seat.id</gewaid>
		<lineno>$!seat.lineno</lineno>
		<rankno>$!seat.rankno</rankno>
		<seatline>$!seat.seatline</seatline>
		<seatrank>$!seat.seatrank</seatrank>
		<loveInd>$!seat.loveInd</loveInd>
		<seattype>$!seat.seattype</seattype>
		<odiid>$!seat.odiid</odiid>
		<price>$!seat.price</price>
		<status>$!seat.status</status>
		<remark>$!seat.remark</remark>
		<costprice>$!seat.costprice</costprice>
		<theatreprice>$!seat.theatreprice</theatreprice>
		<updatetime>$!seat.updatetime</updatetime>
	</openTheatreSeat>
	#end
</openTheatreSeatList>
