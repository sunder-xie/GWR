<data>
	<chargeList>
		#foreach($charge in $chargeList)
		<charge>
			<id>$charge.id</id>
			<tradeno>$charge.tradeNo</tradeno>
			<addtime>$DateUtil.formatTimestamp($charge.addtime)</addtime>
			<validtime>$DateUtil.formatTimestamp($charge.validtime)</validtime>
			<status>$charge.status</status>
			<totalfee>$charge.totalfee</totalfee>
			<memberid>$charge.memberid</memberid>
			<paymethod>$charge.paymethod</paymethod>
			<paybank>$!charge.paybank</paybank>
			<payseqno>$!charge.payseqno</payseqno>
			<outorderid>$!charge.outorderid</outorderid>
			<chargeto>$!charge.chargeto</chargeto>
		</charge>
		#end
	</chargeList>
</data>