<data>
	#if($count)
	<count>$count</count>
	#end
    <videoList>
		#foreach($video in $videoList)
        <video>
			<id>$!video.id</id>
			<url>$!video.url</url>
			<videotitle><![CDATA[$!video.videotitle]]></videotitle>
			<logo>${mobilePath}$!video.logo</logo>
			<content><![CDATA[$!video.content]]></content>
			<addtime>$!video.addtime</addtime>
			<memberid>$!video.memberid</memberid>
			<hotvalue>$!video.hotvalue</hotvalue>
			<clickedtimes>$!video.clickedtimes</clickedtimes>
		</video>
		#end
	</videoList>
</data>