<data>
	<memberList>
		#foreach($info in $infoList)
		#set($age=$ageMap.get($info.id))
		<member>
			<id>$!info.id</id>
			<nickname>$!info.nickname</nickname>
			<headpic>${mobilePath}$!info.headpicUrl</headpic>
			<age>$!age</age>
			<sex>$!info.sex</sex>
			<birthday>$!info.birthday</birthday>
		</member>
		#end
    </memberList>
</data>
