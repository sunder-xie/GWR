<data>
	#if($xmlError)
		<error>$errmsg</error>
	#else
		<nowtime>$!DateUtil.format($!nowtime,'yyyy-MM-dd HH:mm:ss')</nowtime>
		#foreach($qryMap in $qryMapList)
			<order>
				#foreach($key in $qryMap.keySet())
				<$key><![CDATA[$qryMap.get($key)]]></$key>
				#end
				#set($tradeno=$qryMap.get('tradeno'))
				#set($face=$faceMap.get($tradeno))
				#if($VmUtils.isNotEmpObj($face))
					<greetings><![CDATA[$!cpMap.get($order.tradeNo)]]></greetings>
					<venueCnName><![CDATA[$!face.venueCnName]]></venueCnName>
					<venueEnName><![CDATA[$!face.venueEnName]]></venueEnName>
					<showPrice>$!face.showPrice</showPrice>
					<voucherNo>$!face.voucherNo</voucherNo>
					<ticketType>$!face.ticketType</ticketType>
					<programStartTime>$!face.programStartTime</programStartTime>
					<userName><![CDATA[$!face.userName]]></userName>
					<programCnName><![CDATA[$!face.programCnName]]></programCnName>
					<programEnName><![CDATA[$!face.programEnName]]></programEnName>
					<scheduleCnName><![CDATA[$!face.scheduleCnName]]></scheduleCnName>
					<scheduleEnName><![CDATA[$!face.scheduleEnName]]></scheduleEnName>
					<tempPrientInfo><![CDATA[$!face.tempPrientInfo]]></tempPrientInfo>
					<playDate>$!face.playDate</playDate>
					<playTime>$!face.playTime</playTime>
					<stadiumEnName><![CDATA[$!face.stadiumEnName]]></stadiumEnName>
					<enAddress><![CDATA[$!face.enAddress]]></enAddress>
					#if($face.opentype eq 'seat')
						<seatList>
							#foreach($ss in $face.seatList)
							<seat>
								<oneCode>$!ss.oneCode</oneCode>
								<twoCode>$!ss.twoCode</twoCode>
								<priceDescription><![CDATA[$!ss.priceDescription]]></priceDescription>
								<areaCnName><![CDATA[$!ss.areaCnName]]></areaCnName>
								<areaEnName><![CDATA[$!ss.areaEnName]]></areaEnName>
								<areaDescription><![CDATA[$!ss.areaDescription]]></areaDescription>
								<areaIcon>$!ss.areaIcon</areaIcon>
								<ticketPrice>$!ss.ticketPrice</ticketPrice>
								<discountAmount>$!ss.discountAmount</discountAmount>
								<lineno>$!ss.lineno</lineno>
								<rankno>$!ss.rankno</rankno>
								<packInfo><![CDATA[$!ss.packInfo]]></packInfo>
								<packInfo2><![CDATA[$!ss.packInfo2]]></packInfo2>
								<packIntro><![CDATA[$!ss.packIntro]]></packIntro>
								<greetings><![CDATA[$!ss.greetings]]></greetings>
							</seat>
							#end
						</seatList>
					#elseif($face.opentype eq 'stand')
						<standList>
							#foreach($ss in $face.standList)
							<stand>
								<oneCode>$!ss.oneCode</oneCode>
								<twoCode>$!ss.twoCode</twoCode>
								<priceDescription><![CDATA[$!ss.priceDescription]]></priceDescription>
								<areaCnName><![CDATA[$!ss.areaCnName]]></areaCnName>
								<areaEnName><![CDATA[$!ss.areaEnName]]></areaEnName>
								<areaDescription><![CDATA[$!ss.areaDescription]]></areaDescription>
								<areaIcon>$!ss.areaIcon</areaIcon>
								<ticketPrice>$!ss.ticketPrice</ticketPrice>
								<discountAmount>$!ss.discountAmount</discountAmount>
								<serialNum>$!ss.serialNum</serialNum>
								<packInfo><![CDATA[$!ss.packInfo]]></packInfo>
								<packInfo2><![CDATA[$!ss.packInfo2]]></packInfo2>
								<packIntro><![CDATA[$!ss.packIntro]]></packIntro>
								<greetings><![CDATA[$!ss.greetings]]></greetings>
							</stand>
							#end
						</standList>
					#end
				#end
			</order>
		#end
	#end
</data>