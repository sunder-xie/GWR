<data>
	<gewaCityList>
		#foreach($gewaCity in $gewaCityList)
		<gewaCity>
		  <citycode>$!gewaCity.citycode</citycode>
		  <cityname><![CDATA[$!gewaCity.cityname]]></cityname>
		  <provincecode>$!gewaCity.provincecode</provincecode>
		  <provincename><![CDATA[$!gewaCity.provincename]]></provincename>
		  <showHot>$!gewaCity.showHot</showHot>
		  <showIdx>$!gewaCity.showIdx</showIdx>
		  <showAdm>$!gewaCity.showAdm</showAdm>
		  <hotSort>$!gewaCity.hotSort</hotSort>
		  <citySort>$!gewaCity.citySort</citySort>
		  <provinceSort>$!gewaCity.provinceSort</provinceSort>
		  <pinyin>$!gewaCity.pinyin</pinyin>
		  <py>$!gewaCity.py</py>
		  <manmethod>$!gewaCity.manmethod</manmethod>
		  <bpointx>$!gewaCity.bpointx</bpointx>
		  <bpointy>$!gewaCity.bpointy</bpointy>
		</gewaCity>
		#end
	</gewaCityList>
</data>