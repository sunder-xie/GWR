<data>
	<memberInfoList>
		#foreach($memberId in $memberIdList)
		<memberInfo>
			<id>$!memberId</id>
			<nickname><![CDATA[$!cacheMemberMap.get($!memberId).get('nickname')]]></nickname>
			<headpicUrl><![CDATA[$!cacheMemberMap.get($!memberId).get('headpicUrl')]]></headpicUrl>
		</memberInfo>
		#end
	</memberInfoList>
</data>