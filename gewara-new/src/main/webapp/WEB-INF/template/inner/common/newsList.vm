<data>
	<newsList>
		#foreach($news in $newsList)
			<news>
				<id>$!news.id</id>
				<tag>$!news.tag</tag>
				<title><![CDATA[$!news.title]]></title>
				<secondtitle><![CDATA[$!news.secondtitle]]></secondtitle>
				<summary><![CDATA[$!news.summary]]></summary>
				<logo><![CDATA[$!news.logo]]></logo>
				<smallLogo><![CDATA[$!news.smallLogo]]></smallLogo>
				<tplLogo><![CDATA[$!news.tplLogo]]></tplLogo>
				<relatedlink><![CDATA[$!news.relatedlink]]></relatedlink>
				<content><![CDATA[$!news.content]]></content>
				<newstype>$!news.newstype</newstype>
				<flag><![CDATA[$!news.flag]]></flag>
				<releasetime>$!DateUtil.formatTimestamp($!news.releasetime)</releasetime>
				<updatetime>$!DateUtil.formatTimestamp($!news.updatetime)</updatetime>
				<addtime>$!DateUtil.formatTimestamp($!news.addtime)</addtime>
				<relatedid>$!news.relatedid</relatedid>
				<category>$!news.category</category>
				<linksource><![CDATA[$!news.linksource]]></linksource>
				<countycode>$!news.countycode</countycode>
				<newslabel><![CDATA[$!news.newslabel]]></newslabel>
				<author><![CDATA[$!news.author]]></author>
				<categoryid>$!news.categoryid</categoryid>
				<citycode>$!news.citycode</citycode>
				<pagesize>$!news.pagesize</pagesize>
				<clickedtimes>$!news.clickedtimes</clickedtimes>
				<otherinfo><![CDATA[$!news.otherinfo]]></otherinfo>
			</news>
		#end
	</newsList>
</data>