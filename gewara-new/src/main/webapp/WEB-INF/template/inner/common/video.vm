<data>
	<video>
		<id>$!video.id</id>
		<tag>$!video.tag</tag>
		<relatedid>$!video.relatedid</relatedid>
		<category>$!video.category</category>
		<categoryid>$!video.categoryid</categoryid>
		<flag>$!video.flag</flag>
		<url><![CDATA[$!video.url]]></url>
		<videotitle><![CDATA[$!video.videotitle]]></videotitle>
		<logo><![CDATA[$!video.logo]]></logo>
		<content><![CDATA[$!video.content]]></content>
		<addtime>$!DateUtil.formatTimestamp($!video.addtime)</addtime>
		<memberid>$!video.memberid</memberid>
		<verifymemberid>$!video.verifymemberid</verifymemberid>
		<hotvalue>$!video.hotvalue</hotvalue>
		<updatetime>$!DateUtil.formatTimestamp($!video.updatetime)</updatetime>
		<clickedtimes>$!video.clickedtimes</clickedtimes>
	</video>
</data>