<data>
	<card>
		<cardno>$bindCardGroup.cardno</cardno>
		<name>#if($bindCardGroup.cardtype eq 'A')兑换券#else$!{bindCardGroup.cardDiscountAmount}元优惠券#end</name>
		<cardtype>$!bindCardGroup.cardtype</cardtype>
		<usage>$bindCardGroup.usage</usage>
		<discountid>$!bindCardGroup.discountId</discountid>
		<discountAmount>$!bindCardGroup.totalDiscount</discountAmount>
		<totalAmount>$!bindCardGroup.totalAmount</totalAmount>
		<due>$!bindCardGroup.due</due>
	</card>
</data>