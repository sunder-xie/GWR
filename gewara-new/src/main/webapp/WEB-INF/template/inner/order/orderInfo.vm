<data>
	<order>
		<id>$!{order.id}</id>
		<tradeno>$!{order.tradeNo}</tradeno>
		<checkpass>$!{order.checkpass}</checkpass>
		<mobile>$!{order.mobile}</mobile>
		<memberid>$!{order.memberid}</memberid>
		<status>$!{order.status}</status>
		<citycode>$!{order.citycode}</citycode>
		<quantity>$!{order.quantity}</quantity>
		<unitprice>$!{order.unitprice}</unitprice>
		<totalcost>$!{order.totalcost}</totalcost>
		<totalfee>$!{order.totalfee}</totalfee>
		<due>$!{order.due}</due>
		<discount>$!{order.discount}</discount>
		<itemfee>$!{order.itemfee}</itemfee>
		#set($otherInfoMap=$!VmUtils.readJsonToMap($order.otherinfo))
		<otherInfo>
			<realname><![CDATA[$!otherInfoMap.get('realname')]]></realname>
			<idCard><![CDATA[$!otherInfoMap.get('IDcard')]]></idCard>
			<telphone><![CDATA[$!otherInfoMap.get('telphone')]]></telphone>
		</otherInfo>
	</order>
</data>