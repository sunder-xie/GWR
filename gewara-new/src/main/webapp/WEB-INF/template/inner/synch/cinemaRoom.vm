<data>
	<roomList>
		#foreach($room in $roomList)
		<room>
			<id>$!room.id</id>
			<name>$!room.roomname</name>
			<cinemaid>$!room.cinemaid</cinemaid>
			<roomnum>$!room.roomnum</roomnum>
			<linenum>$!room.linenum</linenum>
			<ranknum>$!room.ranknum</ranknum>
			<seatnum>$!room.seatnum</seatnum>
			<roomtype>$!room.roomtype</roomtype>
			<updatetime>$!DateUtil.format($!room.updatetime,'yyyy-MM-dd HH:mm:ss')</updatetime>
		</room>
		#end
	</roomList>
</data>